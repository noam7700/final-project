
function GetPageView(){dataLayer.push({'customerID':(typeof userdata_static==='undefined'||userdata_static=='0')?"":userdata_static,'pageType':window.location.pathname.replace('/pages/',''),'siteType':'desktop-web','pageName':window.location.pathname.replace('/pages/','')});}
function GetPageViewForCatalogPage(){dataLayer.push({'customerID':(typeof userdata_static==='undefined'||userdata_static=='0')?"":userdata_static,'pageType':window.location.pathname.replace('/pages/',''),'siteType':'desktop-web','pageName':window.location.pathname.replace('/pages/',''),'pageVirtualURL':typeof m_sSelectedCategory==='undefined'?'':m_sSelectedCategory,'catalogCategoryID':typeof m_sSelectedCategory==='undefined'?'':m_sSelectedCategory,'bSize':typeof m_footertotalamount==='undefined'?'':m_footertotalamount,'cataloglevel':typeof m_sCurrentDisplayLevel==='undefined'?'':m_sCurrentDisplayLevel});}
function GetVirtualPageMyAcountPage(){dataLayer.push({'customerID':(typeof userdata_static==='undefined'||userdata_static=='0')?"":userdata_static,'pageType':window.location.pathname.replace('/pages/',''),'siteType':'desktop-web','pageName':window.location.pathname.replace('/pages/',''),'pageVirtualURL':$(".MenuItemSelected span").text(),'bSize':typeof m_footertotalamount==='undefined'?'':m_footertotalamount,'event':'GTM page To GA'});}
function GetVirtualPageCatalogPage(){if(flag4VirtualPageCatalogPage)
{dataLayer.push({'customerID':(typeof userdata_static==='undefined'||userdata_static=='0')?"":userdata_static,'pageType':window.location.pathname.replace('/pages/',''),'siteType':'desktop-web','pageName':window.location.pathname.replace('/pages/',''),'pageVirtualURL':m_sSelectedCategory,'catalogCategoryID':m_sSelectedCategory,'cataloglevel':m_sCurrentDisplayLevel,'bSize':typeof m_footertotalamount==='undefined'?'':m_footertotalamount,'event':'GTM page To GA'});}
flag4VirtualPageCatalogPage=false;}
function GTMGetSearchResults(textSearch,resultCount){dataLayer.push({'event':'GTM event To GA','GA_event_category':'use site search','GA_event_action':resultCount,'GA_event_label':textSearch});}
function GTMGetCategory(Category){dataLayer.push({'event':'GTM event To GA','GA_event_category':'Navigation','GA_event_action':'main menu click- level-'+ m_sCurrentDisplayLevel,'GA_event_label':Category});}
function GTMRegistrationSuccess(){dataLayer.push({'event':'GTM event To GA','GA_event_category':'Registration','GA_event_action':'registration success','GA_event_label':'registration success'});}
function GTMOpenProductPage(location,id){dataLayer.push({'event':'GTM event To GA','GA_event_category':'Click on Item','GA_event_action':location,'GA_event_label':id});}
function GTMProductPageOpened(name,id){dataLayer.push({'pageType':'Product Page','pageName':'Product Page','productName':name,'productID':id,'event':'GTM page To GA'});}
function GTMOrderConfirmation(orderID,revenue,jsonOrderItems){dataLayer.push({'event':'ecommerceTransaction','GA_event_category':'Ecommerce','GA_event_action':'Transaction','GA_event_label':'transaction success','ecommerce':{'purchase':{'actionField':{'id':orderID,'revenue':revenue,'tax':'','shipping':'','coupon':''},'products':JSON.parse(jsonOrderItems)}}});}
function GTMOrderConfirmationUpdateMode(id){dataLayer.push({'event':'GTM event To GA','GA_event_category':'Transactions','GA_event_action':'Update order','GA_event_label':id});}
function GTMAddProductToCart(location,productID,productName,price,brand,category,variant,quantity){dataLayer.push({'event':'addToCart','GA_event_category':'Ecommerce','GA_event_action':'Add To Cart','GA_event_label':location,'ecommerce':{'add':{'products':[{'name':productName,'id':productID,'price':price,'brand':brand,'category':category,'variant':variant,'quantity':quantity}]}}});}
function GTMSupplyWindowsCheckout_Open(){dataLayer.push({'event':'GTM event To GA','GA_event_category':'Supply windows','GA_event_action':'Supply windows on checkout - list open','GA_event_label':''});}
function GTMSupplyWindowsCheckout_Select(){dataLayer.push({'event':'GTM event To GA','GA_event_category':'Supply windows','GA_event_action':'Supply windows on checkout - selected','GA_event_label':''});}
function GTMSupplyWindowsCities_Presented(){dataLayer.push({'event':'GTM event To GA','GA_event_category':'Supply windows','GA_event_action':'Supply windows pop up widget presented','GA_event_label':''});}
function GTMSupplyWindowsCities_Select(cityName){dataLayer.push({'event':'GTM event To GA','GA_event_category':'Supply windows','GA_event_action':'Supply windows pop up widget selected','GA_event_label':cityName});}
function GTMSupplyWindowsCatalog_Open(){dataLayer.push({'event':'GTM event To GA','GA_event_category':'Supply windows','GA_event_action':'Supply windows side widget presented','GA_event_label':''});}
function GTMSupplyWindowsCatalog_Select(){dataLayer.push({'event':'GTM event To GA','GA_event_category':'Supply windows','GA_event_action':'Supply windows side widget selected','GA_event_label':'selected'});}
