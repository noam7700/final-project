jQuery("#pwdconfirmerrmsg").html("");$.post(GetCurrentPageUrlString(),{AjaxCallAction:"RecoverPassword",paramEmail:oEmail},function(data){if(data.indexOf('שגיאה: ')==0){ShowErrMsg('שגיאה',data.replace('שגיאה: ',''));return false;}
jQuery("#pwdconfirmerrmsg").html("סיסמא תועבר אליך בדואר אלקטרוני");});return false;}
function WSValidateEmail(email){try{var bResponse=$.ajax({url:GetCurrentPageUrlString(),global:false,type:"POST",data:({AjaxCallAction:"validateemail",ParamEmail:email}),dataType:"text",async:false}).responseText;if(bResponse.indexOf("SessionExpired")==0){DoSessionExpired();return;}
else{return bResponse;}}catch(err){return"False";}}
function EmailIsUsed(mail){var data=WSValidateEmail(mail);var bIsUsed=false;if(data!=null&&data!='undefined'){if(data.toLowerCase()=="false"){bIsUsed=false;}
else{bIsUsed=true;}}
return bIsUsed;}
