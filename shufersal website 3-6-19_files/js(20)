WT_OpenListsCombo('list of choices');else if((ddlBasketMyLists).length==1)
WT_OpenListsCombo('just my list');}
catch(ex){}}
function ValidateIDNumber(idnumber){var sum=GetIDNumberSum(idnumber);sum=sum%10;if(sum>0){return false;}
return true;}
function ValidateIdDigit(sender,args){var idnum=args.Value;if(sumval<1){args.IsValid=false;return;}
var sumval=GetIDNumberSum(idnum);sumval=sumval%10;if(sumval>0){args.IsValid=false;return;}
args.IsValid=true;return;}
function GetIDNumberSum(idnum){idnum=FillIDLeadingZeros(idnum);idnum1=idnum.substr(0,1)*1;idnum2=idnum.substr(1,1)*2;idnum3=idnum.substr(2,1)*1;idnum4=idnum.substr(3,1)*2;idnum5=idnum.substr(4,1)*1;idnum6=idnum.substr(5,1)*2;idnum7=idnum.substr(6,1)*1;idnum8=idnum.substr(7,1)*2;idnum9=idnum.substr(8,1)*1;if(idnum1>9)
idnum1=(idnum1%10)+ 1;if(idnum2>9)
idnum2=(idnum2%10)+ 1;if(idnum3>9)
idnum3=(idnum3%10)+ 1;if(idnum4>9)
idnum4=(idnum4%10)+ 1;if(idnum5>9)
idnum5=(idnum5%10)+ 1;if(idnum6>9)
idnum6=(idnum6%10)+ 1;if(idnum7>9)
idnum7=(idnum7%10)+ 1;if(idnum8>9)
idnum8=(idnum8%10)+ 1;if(idnum9>9)
idnum9=(idnum9%10)+ 1;return idnum1 + idnum2 + idnum3 + idnum4 + idnum5 + idnum6 + idnum7 + idnum8 + idnum9;}
function FillIDLeadingZeros(id){while(id.length<9){id="0"+ id;}
return id;}
