function CheckInputLanguagePopups(e){var isIE=/MSIE/.test(navigator.userAgent);var keyCode=isIE?e.keyCode:e.which;if(keyCode>=1488&&keyCode<=1514){$("div[id$=_pnlToolTip]").show();$('pwdconfirmerrmsg').hide();return;}
$("div[id$=_pnlToolTip]").hide();}
function YSHCatalogCategoriesChoiceField_init(){InitCategories();SetCatalogCategoryChoiceField();}
function SetCatalogCategoryChoiceField(){var SelectedCategories=[];var SelectedCategory=GetCategory(txtCategoryId.value);while(SelectedCategory!=null){SelectedCategories.unshift(SelectedCategory);SelectedCategory=SelectedCategory.PR;}
LoadCatalogCategoryChoiceField(ddlCategory1,Categories,SelectedCategories[1]);LoadCatalogCategoryChoiceField(ddlCategory2,SelectedCategories[1],SelectedCategories[2]);LoadCatalogCategoryChoiceField(ddlCategory3,SelectedCategories[2],SelectedCategories[3]);}
function YSHCatalogCategoriesChoiceField_changed(ddlChanged){var value=ddlChanged.options[ddlChanged.selectedIndex].value;if(ddlChanged.id==ddlCategory1.id){var categ1=GetLvl2Categ(value);LoadCatalogCategoryChoiceField(ddlCategory2,categ1,null);LoadCatalogCategoryChoiceField(ddlCategory3,null,null);}else if(ddlChanged.id==ddlCategory2.id){var categ1=GetLvl2Categ(ddlCategory1.options[ddlCategory1.selectedIndex].value);var categ2=GetLvl3Categ(categ1,value);LoadCatalogCategoryChoiceField(ddlCategory3,categ2,null);if(ddlCategory2.selectedIndex==0)
value=categ1.CN;}else if(ddlChanged.id==ddlCategory3.id){if(ddlCategory3.selectedIndex==0)
value=ddlCategory2.options[ddlCategory2.selectedIndex].value;}
txtCategoryId.value=value;}
function LoadCatalogCategoryChoiceField(ddlCategory,Category,SelectedCategory){ddlCategory.selectedIndex=0;ddlCategory.options.length=1;if(Category==null||Category.CC==null||Category.CC.length==0){ddlCategory.disabled="disabled";return;}
var option=null;for(var index=0;index<Category.CC.length;index++){option=document.createElement("option");option.text=Category.CC[index].DN;option.value=Category.CC[index].CN;if(SelectedCategory!=null&&SelectedCategory.CN==option.value)
option.selected="selected";ddlCategory.add(option);}
ddlCategory.disabled="";}
function InitCategories(Category,ParentCategory){if(arguments.length==0){CategoriesArr=[];CategoriesIds=[];InitCategories(Categories,null);return;}
CategoriesIds.push(Category.CN);CategoriesArr.push(Category);Category.PR=ParentCategory;if(Category.CC!=null)
for(var index=0;index<Category.CC.length;index++)
InitCategories(Category.CC[index],Category);}
function GetCategory(categID){var index=CategoriesIds.indexOf(categID);if(index<0)
return null;return CategoriesArr[index];}
function ValidateQtyForNF(){var oProdQtyCtrl=$('input[id$=prodpageqtybox]');var oProdQty=parseFloatFixed(oProdQtyCtrl.val());if(0 + oProdQty>1&&0 + oProdQty<100&&($('.ddlSizes').length>0||$('.ddlColors').length>0)){ShowErrMsg('הודעה','.'+'לרכישת מוצרים נוספים במידות/צבעים שונים, נא לציין את המאפיינים הנוספים בחלון ההערה');}
return true;}
function getval(sel){var prop=sel.className=='ddlColors'?"צ'-":"מ'-";if(sel.value!='בחר'&&$('.number').val()==1){var temp=$('.txtnoborder').val();var subtemp=temp.indexOf(prop)>=0?temp.substr(temp.indexOf(prop),temp.length):temp;subtemp=temp.indexOf(prop)>=0?subtemp.substr(subtemp.indexOf(prop),subtemp.indexOf(".")+ 1-subtemp.indexOf(prop)):temp;temp=temp.indexOf(prop)>=0?temp.replace(subtemp,''):temp;$('.txtnoborder').val(prop + sel.value +'.'+ temp);}}
