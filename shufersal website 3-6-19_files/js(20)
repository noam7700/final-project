function iqa(prodID){var Qty=$("input[id='ProdAddQuantityTextBox_"+ prodID +"']");var dQuantity=parseFloatFixed(Qty.val());var increase=parseFloat(Qty.attr('iq'));if(isNaN(increase))
increase=1;if(!ValidateMaxQty(dQuantity + increase,GetMaxQtyByUM(GetUnitOfMeasureById('#ddlUMChartViewAdd_'+ prodID,'#divUMChartView_'+ prodID))))return;var myval=0 + dQuantity + increase;Qty.val(myval.toFixed(2));}
