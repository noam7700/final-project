function BuildMenuLvl2Html(categID,subID){if(categID==950000)return html='<div class="sccb" id='+ subID +' level="2"></div>';var categObj=GetLvl2Categ(categID);if(categObj==null)
return'';var minW=(parseInt(categObj.CC.length/2)+ categObj.CC.length%2)*120;var html='';if(Ie9())
html='<div class="sccb" id="'+ subID +'" level="2">';else
html='<div class="sccb" id="'+ subID +'" level="2">';for(var index=0;index<categObj.CC.length;index +=2){if(categObj.CC[index]!=undefined&&HideCatArr.indexOf(categObj.CC[index].CN)<0){html +='<div class="scc"><a lv1cat="'+ categObj.CN +'" lv2cat="'+ categObj.CC[index].CN +'" href="#" onclick="SCS(this,\''+ categObj.CN +'\',\''+ categObj.CC[index].CN +'\',2,\'0\', \''+ categObj.CC[index].CN +'\', false);">'+ categObj.CC[index].DN +'</a></div>';}
if(categObj.CC[index + 1]!=undefined&&HideCatArr.indexOf(categObj.CC[index + 1].CN)<0){html +='<div class="scc"><a lv1cat="'+ categObj.CN +'" lv2cat="'+ categObj.CC[index + 1].CN +'" href="#" onclick="SCS(this,\''+ categObj.CN +'\',\''+ categObj.CC[index + 1].CN +'\',2,\'0\', \''+ categObj.CC[index + 1].CN +'\', false);">'+ categObj.CC[index + 1].DN +'</a></div>';}}
html +='</div>';return html;}
