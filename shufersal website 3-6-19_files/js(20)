function SendToFriend(){var friendName=$("input[id$=_txtName]").val();var friendLastName=$("input[id$=_txtLastName]").val();var friendEmail=$("input[id$=_txtEmail]").val();var friendPhone=$find("ctl00_PlaceHolderMain_ucMain_ucSendToFriend_ddlPhonePrefix")._value + $("input[id$=_txtPhoneNumber]").val();var friendClientId=$("input[id$=_txtClientNo]").val();$.post(GetCurrentPageUrlString(),{AjaxCallAction:"SendToFriend",Name:friendName,LastName:friendLastName,Email:friendEmail,Phone:friendPhone,ClientId:friendClientId},function(data){if(data.indexOf("SessionExpired")==0){DoSessionExpired();return;}
if($("#ctl00_PlaceHolderMain_ucMain_pnlBussinessUser").is(":visible")){CloseSendToFriendBC();if(data.toLowerCase()=="true"){$("#ctl00_PlaceHolderMain_ucMain_litSendToFriendRegMessageBCFailed").css("display","none")
$("#ctl00_PlaceHolderMain_ucMain_litSendToFriendRegMessageBCSuccess").css("display","inline")}
else{$("#ctl00_PlaceHolderMain_ucMain_litSendToFriendRegMessageBCSuccess").css("display","none")
$("#ctl00_PlaceHolderMain_ucMain_litSendToFriendRegMessageBCFailed").css("display","inline")}}
else{CloseSendToFriend();if(data.toLowerCase()=="true"){$("#ctl00_PlaceHolderMain_ucMain_litSendToFriendRegMessageFailed").css("display","none")
$("#ctl00_PlaceHolderMain_ucMain_litSendToFriendRegMessageSuccess").css("display","inline")}
else{$("#ctl00_PlaceHolderMain_ucMain_litSendToFriendRegMessageSuccess").css("display","none")
$("#ctl00_PlaceHolderMain_ucMain_litSendToFriendRegMessageFailed").css("display","inline")}}});}
