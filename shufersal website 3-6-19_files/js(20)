sSmallImage=$(oSmallImage).attr("SmallImage");var oPrice=$("div[id='divBasketProductPriceNormal_"+ sProdID +"']")[0];if(oPrice!=null)
sPrice=oPrice.innerHTML;var oPriceBefore=$("div[id='divBasketProductPriceDiscount_"+ sProdID +"']")[0];if(oPriceBefore!=null)
sPriceBefore=oPriceBefore.innerHTML;sPriceBefore=oPriceBefore.innerHTML;var oLabel=$("div[id='divBasketProductPrice_"+ sProdID +"']")[0];if(oLabel!=null)
sLabel=$(oLabel).attr("sLabel");if(oSmallImage!=null)
bSaveMoreImage=$(oSmallImage).attr("SaveMore");else
bSaveMoreImage="false";var oCategoryName=$("div[id='divBasketCategoryName_"+ sCatID +"']")[0];if(oCategoryName!=null)
sCategoryName=$(oCategoryName).attr("CatName");var oQuant=document.getElementById("txtBasketProductQuant_"+ sProdID);if(oQuant!=null)
sQuant=oQuant.value;var oUnits=$("div[id='divBasketSeparatorUnit_"+ sProdID +"']")[0];sUnits=oUnits.innerText;AddProductToOpenList(sProdID,sCatID,sProductName,sPrice,sPriceUnit,sPriceBefore,sLabel,sSmallImage,sCategoryName,bSaveMoreImage,sQuant,sUnits);}
