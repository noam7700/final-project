function ValidateCallMeForm(){var bUserIsLoggedIn=$('input[id$=_ucCallMe_txtPhoneLoggedIn]').length>0;var vldNameExpr=$('input[id$=hdnCallMeNameExp]').val();var vldPhoneExpr=$('input[id$=hdnCallMePhoneExp]').val();if(bUserIsLoggedIn){var phone=$('#ctl00_PlaceHolderMain_ucPopupMasterLoader_ucCallMe_txtPhoneLoggedIn_text');if(phone.val().match(vldPhoneExpr)==null){$('#lblPhoneLoggenInErr').text('נא למלא את השדות המסומנים').show();return false;}
else{$('#lblPhoneLoggenInErr').text('').hide();}
return true;}
else{var name=$('#ctl00_PlaceHolderMain_ucPopupMasterLoader_ucCallMe_txtName_text');var phone=$('#ctl00_PlaceHolderMain_ucPopupMasterLoader_ucCallMe_txtPhone_text');if(phone.val().match(vldPhoneExpr)==null){$('#txtCallMePhoneErr').text('נא למלא את השדות המסומנים').show();return false;}
else{$('#txtCallMePhoneErr').text('').hide();}
if(name.val().match(vldNameExpr)==null){$('#txtCallMeNameErr').text('נא למלא את השדות המסומנים').show();return false;}
else{$('#txtCallMeNameErr').text('').hide();}
return phone.val().match(vldPhoneExpr)!=null&&name.val().match(vldNameExpr)!=null;}
return false;}
