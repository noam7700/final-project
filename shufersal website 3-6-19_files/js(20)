function ProdPageChangeQty(value,bChange){if(value!=null){var oProdQtyCtrl=$('input[id$=prodpageqtybox]');var oProdQty=parseFloatFixed(oProdQtyCtrl.val());var increment=parseFloat(oProdQtyCtrl.attr('iq'));if(isNaN(increment))increment=1;var unitMeasure=GetUnitOfMeasureById('#ddlUMProdPage','#divUMProdPage');if(!bChange){if(value==1)
oProdQty=oProdQty + increment;if(value==-1)
oProdQty=oProdQty-increment;}
else{if(oProdQty>0&&(oProdQty*100)%(increment*100)!=0){ShowErrMsg('שגיאה','כמות צריכה להיות חיובית ובמספרים שלמים '+ increment);oProdQty=increment;}}
if(oProdQty<=0){oProdQty=0;}
if(!ValidateMaxQty(oProdQty,GetMaxQtyByUM(unitMeasure)))return;var myval=0 + oProdQty;if(value==-1&&$('div[id$=btnprodpageadd]:visible').length>0){if(myval>0)
oProdQtyCtrl.val(oProdQty.toFixed(2));}
else{oProdQtyCtrl.val(oProdQty.toFixed(2));}}}
function DinersChangeQty(value,bChange,increment){if(value!=null){var oProdQtyCtrl=$('input[id$=prodpageqtybox]');var oProdQty=parseFloatFixed(oProdQtyCtrl.val());if(isNaN(increment))increment=1;if(!bChange){if(value==1)
oProdQty=oProdQty + increment;if(value==-1)
oProdQty=oProdQty-increment;}
else{if(oProdQty>0&&(oProdQty*100)%(increment*100)!=0){ShowErrMsg('שגיאה','הכמות צריכה להיות חיובית ובכפולת הסועדים '+ increment);oProdQty=increment;}}
if(oProdQty<=0){oProdQty=0;}
var myval=0 + oProdQty;if(value==-1&&$('div[id$=btnprodpageadd]:visible').length>0){if(myval>0){oProdQtyCtrl.val(oProdQty);var products=$.getQueryString('products');var z=products.split(';');$.each(z,function(index,value){a=value.split(':')
AddToBasketListQtyDiners(a[0],a[1]*(oProdQty/increment),0);});}}
else{oProdQtyCtrl.val(oProdQty);var products=$.getQueryString('products');var z=products.split(';');$.each(z,function(index,value){a=value.split(':')
AddToBasketListQtyDiners(a[0],a[1]*(oProdQty/increment),0);});}}}
