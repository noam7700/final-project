function ismaxlength(obj,maxlength){if(maxlength==null||typeof(maxlength)=='undefined'){maxlength=TEXT_AREA_MAXLENGTH}
if(obj.getAttribute&&obj.value.length>maxlength)
obj.value=obj.value.substring(0,maxlength)}
