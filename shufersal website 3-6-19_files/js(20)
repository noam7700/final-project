function AllowDivur(flagForAdvertisingInformation,DigitalCommunicationsUrl){if(flagForAdvertisingInformation&&!$('#chkRememberNew2').is(':checked')){$('#chkRememberNewtext').show();return false;}
if(typeof isDivurFromBanner!="undefined"&&isDivurFromBanner){if(flagForAdvertisingInformation){$.get(DigitalCommunicationsUrl,function(data){if(data.indexOf('false')==0){ShowDirbunForCCPopup(6,6);$('#popupBoxWrapper').css('width','632px');return;}
else{if(flagForAdvertisingInformation){var oPopup=CustomPopup(".popupBox",'/_layouts/Shufersal_Pages/popup_loader.aspx?key=4&actionBtn=4');oPopup.close();if(!IsExtended()){HideGeneralLoadingPanel();}else{InvertIndex();}
ShowDirbunForCCPopup(7,7);$('#popupBoxWrapper').css('width','632px');}}});}}else{$.post(GetCurrentPageUrlString(),{AjaxCallAction:"AllowDivur",AdvertisingInformation:flagForAdvertisingInformation},function(data){if(data.indexOf("SessionExpired")==0){DoSessionExpired();return;}
else{if(data.indexOf('שגיאה: ')==0){ShowDirbunForCCPopup(6,6);$('#popupBoxWrapper').css('width','632px');return;}
else{if(flagForAdvertisingInformation){var oPopup=CustomPopup(".popupBox",'/_layouts/Shufersal_Pages/popup_loader.aspx?key=4&actionBtn=4');oPopup.close();if(!IsExtended()){HideGeneralLoadingPanel();}else{InvertIndex();}
ShowDirbunForCCPopup(7,7);$('#popupBoxWrapper').css('width','632px');}}}});}};function AllowDivur2(flagForAdvertisingInformation){if(flagForAdvertisingInformation&&!$('#chkRememberNew3').is(':checked')){$('#chkRememberNewtext2').css('visibility','visible');return false;}
$.post(GetCurrentPageUrlString(),{AjaxCallAction:"AllowDivur",AdvertisingInformation:flagForAdvertisingInformation},function(data){if(data.indexOf("SessionExpired")==0){DoSessionExpired();return;}
else{if(data.indexOf('שגיאה: ')==0){ShowErrMsg('שגיאה',data.replace('שגיאה: ',''));return;}
else{if(flagForAdvertisingInformation){var oPopup=CustomPopup(".popupBox",'/_layouts/Shufersal_Pages/popup_loader.aspx?key=4&actionBtn=4');oPopup.close();if(!IsExtended()){HideGeneralLoadingPanel();}else{InvertIndex();}}}}});};function OpenDirbun(src,link){var oPopup=CustomPopup(".popupBox",'/_layouts/Shufersal_Pages/Dirbun.aspx?src='+ src +"&link="+ link)
oPopup.callback_container="popup_content";if(oPopup){currentPopupFunction='DIRBUN';oPopup.post('',null,function(){$(".genericPopupClose2").click(function(){oPopup.close();if(!IsExtended()){HideGeneralLoadingPanel();}else{InvertIndex();}});});}}
