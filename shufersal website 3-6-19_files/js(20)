function CloseGenericConfirm(){var oMask=$("#popupBoxMask");if(oMask.length>0){oMask.css({"width":jQuery(window).width()+"px","height":jQuery(window).height()+"px","z-index":"999"}).show();if(bCloseAlertMask)
oMask.css("display","none");}
var oAlert=jQuery("#popupBoxWrapperConfirm");if(oAlert.length>0){oAlert=jQuery("#popupBoxWrapperConfirm").css("display","none");$("#genericAlertText").empty();}
if($.browser.msie){jQuery("#popupieshadow").remove();}}
$('input[name="rptPaymentRadio"]').live('click',function(){($(this).closest('table').find('input.rptPayInput')).attr('disabled',true);($(this).closest('tr').find('input.rptPayInput')).attr('disabled',false).focus();});$(".rptPayInput").live('keydown',function(e){if($.inArray(e.keyCode,[46,8,9,27,13,110,190])!==-1||(e.keyCode==65&&e.ctrlKey===true)||(e.keyCode==67&&e.ctrlKey===true)||(e.keyCode==88&&e.ctrlKey===true)||(e.keyCode>=35&&e.keyCode<=39)){return;}
if((e.shiftKey||(e.keyCode<48||e.keyCode>57))&&(e.keyCode<96||e.keyCode>105)){e.preventDefault();}});function isEmpty(str){return(!str||0===str.length);}
function CheckChangeUserDetails(){var email=jQuery('input[id$="txtEmailBox"]').val();var idNumber=jQuery('input[id$="txtIDBox"]').val();if(isEmpty(email)||isEmpty(idNumber)){$("#rptPayReq").show();return;}
else{ShowTopGeneralLoadingPanel();$("#rptPayReq").hide();$.post(GetCurrentPageUrlString(),{AjaxCallAction:"CheckChangeUserDetails",email:email,idNumber:idNumber},function(data){if(data.indexOf("SessionExpired")==0){DoSessionExpired();return;}else
switch(data){case"0":window.location.href="/pages/homepage.aspx";break;case"1":ChangeUserDetails();break;case"2":ShowErrMsg('שגיאה','חלה שגיאה באימות ');break;}
HideGeneralLoadingPanel();});}}
