function AddPopupShaddow(oCtrl){if($.browser.msie){var oShadowLeft=((jQuery(window).width()-oCtrl.width())/2);if(!Ie7()){var pos=oCtrl.position();oCtrl.after("<div class='ie-shadow' id='popupieshadow'></div>");jQuery("#popupieshadow").width(jQuery(oCtrl).width()).height(jQuery(oCtrl).height()).css({"left":pos.left + 7 +"px","top":pos.top +"px","display":"block","position":"fixed","z-index":"1000","background":"#000","filter":"progid:DXImageTransform.Microsoft.Blur(PixelRadius='4', MakeShadow='true', ShadowOpacity='0.4')"});}
else{oCtrl.css("width","220px");var pos=oCtrl.position();oCtrl.after("<div class='ie-shadow' id='popupieshadow'></div>");jQuery("#popupieshadow").width(jQuery(oCtrl).width()).height(jQuery(oCtrl).height()).css({"left":oCtrl.css("left"),"top":(pos.top)+"px","display":"block","position":"fixed","z-index":"1000","background":"#000","filter":"progid:DXImageTransform.Microsoft.Blur(PixelRadius='4', MakeShadow='true', ShadowOpacity='0.4')"});}}else{oCtrl.css({"-moz-box-shadow":"rgba(0,0,0,0.50) 5px 5px 10px 0px","-webkit-box-shadow":"rgba(0,0,0,0.50) 5px 5px 10px","box-shadow":"rgba(0,0,0,0.50) 5px 5px 10px 0px"});}}
