else{document.getElementById(sIDRight).className=document.getElementById(sIDRight).className +"Selected";}}
if(document.getElementById(sIDCenter)!=null){if(isSelect){document.getElementById(sIDCenter).className=document.getElementById(sIDCenter).className.replace("Selected","")+" TDBlackText";document.getElementById(sIDCenter).className=document.getElementById(sIDCenter).className.replace("TDWhiteText","")}
else{document.getElementById(sIDCenter).className=trim(document.getElementById(sIDCenter).className).replace("TDBlackText","");document.getElementById(sIDCenter).className=trim(document.getElementById(sIDCenter).className)+"Selected"+" TDWhiteText";}}}
