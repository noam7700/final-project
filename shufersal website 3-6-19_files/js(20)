function ProductLineUpdateToBasketCallback(data,iProdID){var oBasketDiv=$('#dvMinibasketContent');oBasketDiv.empty();$(data).appendTo(oBasketDiv);if(CheckDisplayModeThumb()){ProductActionHighlight(iProdID,'הפריט התעדכן בסל');}
else{ProductLineActionHighlight(iProdID);}
ScrollToCategMiniBasket(GetCategIdFromMiniBasketProductRow(iProdID),true);var oAprovalBox=oBasketDiv.find("div[class^='productRow'][pid='"+ iProdID +"'] div.miniBasketQ");oAprovalBox.unbind("click");$(document).unbind("keypress");$(document).unbind("click");oAprovalBox.find("span[class^='QAprove']").css("display","none");var mainRowControl=oBasketDiv.find("div[class^='productRow'][pid='"+ iProdID +"']");var sImgPath=mainRowControl.find("img").attr("src");$("#productUpdateMessage").find("img").attr("src",sImgPath);$("#productUpdateMessage").css("display","inline");oAprovalBox.attr("class","miniBasketQ miniBasketQUpdated");$("<span class='check'></span><span class='updatedMessage'>הפריט המעודכן</span>").appendTo(oAprovalBox);oAprovalBox=oBasketDiv.find("div[class^='productRow'][pid='"+ iProdID +"'] div.miniBasketQUpdated");oAprovalBox.parent().find(".productLink").css("margin-right","-7px");var oOutOfStockDiv=oBasketDiv.find("div[class^='productRow'][pid='"+ iProdID +"'] div.outOfStockDiv");var oOutOfStockMask=oBasketDiv.find("div[class^='productRow'][pid='"+ iProdID +"'] div.outOfStockMask");if(oOutOfStockDiv!=null&&oOutOfStockMask!=null){oOutOfStockDiv.css("margin-top","-70px");oOutOfStockMask.css("margin-top","-70px");}
AppendEmptyRowsToMiniBasket();setTimeout("RestoreAproval('"+ iProdID +"');",2000);var oQtyBox=$("tr[grdline_rowprodid='"+ iProdID +"'] #prodlineqtybox_"+ iProdID);oQtyBox.attr('oldqty',oQtyBox.val());}
function IncreaseProdLineQuantity(prodID){var Qty=$("tr[grdline_rowprodid='"+ prodID +"'] #prodlineqtybox_"+ prodID);var dQuantity=parseFloatFixed(Qty.val());var oTXT=$("#prodlineqtybox_"+ prodID);var increase=parseFloat(oTXT.attr('iq'));if(isNaN(increase))
increase=1;if(!ValidateMaxQty(dQuantity + increase,GetMaxQtyByUM(GetUnitOfMeasureById('#ddlUMLineView_'+ prodID,'#divUMLineView_'+ prodID))))return;var myval=0 + dQuantity + increase;oTXT.val(myval.toFixed(2));}
function DecreaseProdLineQuantity(prodID){var Qty=$("tr[grdline_rowprodid='"+ prodID +"'] #prodlineqtybox_"+ prodID);var dQuantity=parseFloatFixed(Qty.val());var oTXT=$("#prodlineqtybox_"+ prodID);var increase=parseFloat(oTXT.attr('iq'));if(isNaN(increase))
increase=1;var myval=0 + dQuantity-increase;if(myval>0)
oTXT.val(myval.toFixed(2));}
function IncreaseProdRangeQuantity(prodID,fieldID,gridID){$("#divPromoValidateWarning").hide();var Qty=$("tr[grdRange_rowprodid='"+ prodID +"'] #"+ fieldID +"_"+ prodID);var dQuantity=parseFloatFixed(Qty.val());var increase=parseFloat(Qty.attr('iq'));if(isNaN(increase))
increase=1;if(isNaN(dQuantity)){dQuantity=0;}
var myval=0 + dQuantity + increase;if(!ValidateMaxQty(myval,GetMaxQtyByUM(GetUnitOfMeasureById('#ddlUMRangeView_'+ prodID,'#divUMRangeView_'+ prodID))))return;Qty.val(myval.toFixed(2));SynchronizeRowTotal(prodID,gridID,myval);SynchronizeTotal(gridID,fieldID);}
function DecreaseProdRangeQuantity(prodID,fieldID,gridID){$("#divPromoValidateWarning").hide();var Qty=$("tr[grdRange_rowprodid='"+ prodID +"'] #"+ fieldID +"_"+ prodID);var dQuantity=parseFloatFixed(Qty.val());var increase=parseFloat(Qty.attr('iq'));if(isNaN(increase))
increase=1;if(isNaN(dQuantity))
dQuantity=0;var myval=0 + dQuantity-increase;if(myval>=0){Qty.val(myval.toFixed(2));SynchronizeRowTotal(prodID,gridID,myval);SynchronizeTotal(gridID,fieldID);}}
function ChangeProdRangeQuantity(prodID,fieldID,gridID,dQty){$("#divPromoValidateWarning").hide();var Qty=$("tr[grdRange_rowprodid='"+ prodID +"'] #"+ fieldID +"_"+ prodID);var increase=parseFloatFixed(Qty.attr('iq'));if(isNaN(increase))
increase=1;var unitMeasure=GetUnitOfMeasureById('#ddlUMRangeView_'+ prodID,'#divUMRangeView_'+ prodID);var myval=0 + parseFloat(dQty);var maxqty=GetMaxQtyByUM(unitMeasure);if(maxqty==null||maxqty=='undefined')
maxqty=ITEM_MAX_QTY;if(0 + myval>parseFloat(maxqty)){ShowErrMsg('הודעה','.'+' כמות מקסימלית לפריט זה היא '+ String(maxqty));return;}
if(dQty>0&&((dQty*100)%(increase*100)==0||unitMeasure.charAt(0)!='י')){Qty.val(myval.toFixed(2));}
else{ShowErrMsg('שגיאה','כמות צריכה להיות חיובית ובמספרים שלמים '+ increase);Qty.val(increase);}
SynchronizeRowTotal(prodID,gridID,myval);SynchronizeTotal(gridID,fieldID);}
function SynchronizeRowTotal(prodID,gridID,qty){var totalPrice=$("div[id$='"+ gridID +"'] tr[grdRange_rowprodid='"+ prodID +"'] span[rowtotalprice='1']");var actualPrice=$("tr[grdRange_rowprodid='"+ prodID +"']").attr("grdRange_rowprodprice");if(isNaN(actualPrice))
actualPrice=0;var totalpriceval=0 + parseFloat(actualPrice)*qty;totalPrice.text(totalpriceval.toFixed(2));}
function SynchronizeTotal(gridtosearch,fieldid){var grdProdsToBuy=$("div[id$='"+ gridtosearch +"']");var totalval=0;if(grdProdsToBuy.find("input[id^='"+ fieldid +"']").length>0){grdProdsToBuy.find("input[id^='"+ fieldid +"']").each(function(iIndex,oElem){var boxQty=parseFloat($(oElem).val());if(isNaN(boxQty))
boxQty=0;totalval +=0 + boxQty;});}
var Total=$("div[id$='"+ gridtosearch +"'] span[id='"+ gridtosearch +"Total']");Total.text(totalval);}
function ValidatePromosQty(){var TotalProdsToBuy=$("div[id$='"+"grdProdsToBuy"+"'] span[id='"+"grdProdsToBuy"+"Total']");var TotalProdsToGet=$("div[id$='"+"grdProdsToGet"+"'] span[id='"+"grdProdsToGet"+"Total']");var bReturn=true;if(TotalProdsToBuy.length>0&&(parseFloat(TotalProdsToBuy.text())<parseFloat(TotalProdsToBuy.attr("promoqty")))&&parseFloat(TotalProdsToBuy.attr("promoqty"))>0){if($("#divSelRangeAddButton").attr('isvalidated')!='1')
bReturn=false;$("#divSelRangeAddButton").attr('isvalidated','1');$("#divPromoValidateWarning").show();}
if(TotalProdsToGet.length>0&&(parseFloat(TotalProdsToGet.text())<parseFloat(TotalProdsToGet.attr("promoqty")))&&parseFloat(TotalProdsToGet.attr("promoqty"))>0){if($("#divSelRangeAddButton").attr('isvalidated')!='1')
bReturn=false;$("#divSelRangeAddButton").attr('isvalidated','1');$("#divPromoValidateWarning").show();}
return bReturn;}
function GetValueNotNull(value,defaultval){if(isNaN(value))
return defaultval;return value;}
function SelRangeAddToBasket(){if(!ValidatePromosQty())
