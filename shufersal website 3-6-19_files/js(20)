var validCC_result,validCC_number,validCC_numlength,validCC_prefix,validCC_cardnumber,validCC_sum,validCC_cardtype;function Mid(str,start,len){if(start<0||len<0)return"";var iEnd,iLen=String(str).length;if(start + len>iLen)
iEnd=iLen;else
iEnd=start + len;return String(str).substring(start,iEnd);}
function checkdigit(){var i,incsum,twice;validCC_sum=0;twice=false;for(i=validCC_numlength-1;i>=0;i--){if(twice==false){validCC_sum=validCC_sum + parseInt(Mid(validCC_number,i,1));twice=true;}
else{incSum=parseInt(Mid(validCC_number,i,1))*2;if(incSum>9)
incSum=(incSum%10)+ parseInt(incSum/10);validCC_sum=validCC_sum + incSum;twice=false;}}
return((validCC_sum%10)==0);}
function checkIsraDigit(){var validCC_sum,temp;temp=validCC_number;if(validCC_numlength==8){temp="0"+ temp;}
validCC_sum=parseInt(Mid(temp,0,1))*9 + parseInt(Mid(temp,1,1))*8 + parseInt(Mid(temp,2,1))*7 + parseInt(Mid(temp,3,1))*6 + parseInt(Mid(temp,4,1))*5 + parseInt(Mid(temp,5,1))*4 + parseInt(Mid(temp,6,1))*3 + parseInt(Mid(temp,7,1))*2 + parseInt(Mid(temp,8,1))*1;return((validCC_sum%11)==0);}
function checkIsraCard(){var temp,tempprefix;validCC_result=false;if(validCC_numlength!=8&&validCC_numlength!=9&&validCC_numlength!=16)return validCC_result;temp=validCC_number;if(validCC_numlength==8)temp="0"+ temp;if(validCC_numlength==8||validCC_numlength==9){tempprefix=Mid(temp,0,2);if(tempprefix=="00"||tempprefix=="52"||Mid(temp,1,1)=="7"||Mid(temp,0,1)=="6")return validCC_result;}
else{if(Mid(validCC_number,0,1)!="5")return validCC_result;validCC_result=checkdigit();return validCC_result;}
return checkIsraDigit();}
function checkVisa(){validCC_result=false;if(validCC_numlength!=13&&validCC_numlength!=16)return validCC_result;if(validCC_numlength==13)
if(Mid(validCC_number,0,1)!="4")return validCC_result;if(validCC_numlength==16)
if(Mid(validCC_number,0,1)!="3"&&Mid(validCC_number,0,1)!="4")return validCC_result;return checkdigit();}
function checkDinners(){validCC_result=false;if(validCC_numlength!=14)return;if((Mid(validCC_number,0,1)!="3")&&(Mid(validCC_number,0,1)!="6"))return validCC_result;return checkdigit();}
function checkAmerican(){validCC_result=false;if(validCC_numlength!=15)return;if(validCC_prefix!="34"&&validCC_prefix!="37")return validCC_result;return checkdigit();}
function illegal(){var billegal=true;if(validCC_number=="4111111111111111"||validCC_number=="5111111111111118"||validCC_number=="4580458045804580")
billegal=false;return billegal;}
function validCard(validCC_cardnumber,cardType){validCC_result=false;validCC_number=validCC_cardnumber;validCC_numlength=validCC_number.toString().length;validCC_cardtype=cardType;if(validCC_numlength!=0){validCC_prefix=Mid(validCC_number,0,2)
switch(validCC_cardtype){case"1":validCC_result=checkVisa();break;case"2":validCC_result=checkVisa();break;case"3":validCC_result=checkIsraCard();break;case"4":validCC_result=checkIsraCard();break;case"5":validCC_result=checkAmerican();break;case"6":validCC_result=checkDinners();break;default:validCC_result=illegal();break;}}
return validCC_result&&illegal();}
function DoCreditcardValidation(creditCardTypeCtrl,creditCardNumberCtrl,vldCtrl,source,arguments){var cardType=creditCardTypeCtrl.val();var cardNumber=creditCardNumberCtrl.val();if(cardType!=undefined&&cardNumber!='undefined'&&cardType!=''&cardNumber!=''){var vldExpr=source.getAttribute("VldExp");var re=new RegExp(vldExpr);var bIsValid=validCard(cardNumber,cardType);var bIsUpdate=$('div[id$=divIsUpdate]').text();if(bIsUpdate!=''&&bIsUpdate.toLowerCase()=="true"){bIsValid=true;}
if(bIsValid){if(arguments){arguments.IsValid=true;}
vldCtrl.hide();}
else{arguments.IsValid=false;vldCtrl.show();}}
else{if(arguments){arguments.IsValid=false;}
vldCtrl.show();}}
