oPopup.css({"top":oElement.offset().top + iAddedTop,"left":(oElement.offset().left==0?oElement.width()-150 + iAddedLeft:oElement.offset().left)-350 + iAddedLeft});var menuWrapper=$('#divCategoryBgV2');var menuDiv=menuWrapper.find("div.cia:first");if(step==2){menuDiv.show();menuDiv.parent().parent().css("margin-top","-10px");ShowPopup(null,menuDiv.parent().parent().attr("itemid"));}
else{menuDiv.parent().parent().css("margin-top","0px");menuDiv.hide();}
