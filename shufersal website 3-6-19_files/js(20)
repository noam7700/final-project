else{$('#dvChartFilter').find('.FilterBtnMiddle').text('סינון פעיל ');$('#dvChartFilter').find('.FilterBtnMiddle').append("<div id='dvCancelFilter' class='filterCANCEL' onclick='OnClearFilterClick();'>x</div>");$('#dvChartFilter').find('.FilterBtnMiddle').css("color","#D31242").css("font-weight","bold");$('#dvListFilter').find('.FilterBtnMiddle').text('סינון פעיל ');$('#dvListFilter').find('.FilterBtnMiddle').append("<div id='dvCancelFilter' class='filterCANCEL' onclick='OnClearFilterClick();'>x</div>");$('#dvListFilter').find('.FilterBtnMiddle').css("color","#D31242").css("font-weight","bold");}}
