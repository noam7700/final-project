else{jQuery.get('/_layouts/Shufersal_Pages/BasketHelpPopup.aspx?BasketInfoStep='+ step,function(data){jQuery("#BasketHelpPopupContainer").html(data);var oPopup=jQuery(".BasketHelpPopup_productNoteTooltip");if(step=='0'){oPopup=jQuery(".BasketHelpPopup_productNoteTooltip2");oPopup.center({minX:-350})}
