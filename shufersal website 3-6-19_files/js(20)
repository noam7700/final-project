else{if(data.indexOf('שגיאה: ')==0){ShowErrMsg('שגיאה',data.replace('שגיאה: ',''));HideBasketDetailsLoadingPanel();return;}
RefreshDetailedBasket('divOpenListContent');if(CheckDisplayModeThumb()){ProductActionRemoveHighlight(iProductID);}
else{ProductLineActionRemoveHighlight(iProductID);}
ChangePromoDisplayMode(false,iProductID,0);RefreshOpenListFooter();setTimeout("HideBasketDetailsLoadingPanel();",2000);}});return false;}
function DecreaseTextBoxQuantityOpenList(iProductID,bGoToBasket){HighlitOpenListProduct(iProductID);var oProdQtyCtrl=$("#txtOpenListQty_"+ iProductID);var oProdQty=parseFloatFixed(oProdQtyCtrl.val());var increment=parseFloat(oProdQtyCtrl.attr('iq'));if(isNaN(increment))increment=1;oProdQty=oProdQty-increment;if(oProdQty<=0){oProdQty=0;}
oProdQtyCtrl.val(oProdQty.toFixed(2));OpenListOnQuantChange(iProductID,oProdQty);}
function IncreaseTextBoxQuantityOpenList(iProductID,bGoToBasket){HighlitOpenListProduct(iProductID);var oProdQtyCtrl=$("#txtOpenListQty_"+ iProductID);var oProdQty=parseFloatFixed(oProdQtyCtrl.val());var increment=parseFloat(oProdQtyCtrl.attr('iq'));if(isNaN(increment))increment=1;oProdQty=oProdQty + increment;if(!ValidateMaxQty(oProdQty,GetMaxQtyByUM(GetUnitOfMeasureById('#ddlUMExtBasket_'+ iProductID,'#divUMExtBasket_'+ iProductID))))return;oProdQtyCtrl.val(oProdQty.toFixed(2));OpenListOnQuantChange(iProductID,oProdQty);}
function OpenListOnQuantChange(oID,dQuantity){}
function HighlitOpenListProduct(sProductID){var oObjectClicked=$('#OpenListProduct_'+ sProductID);if(lastProductClicked!=sProductID){if(lastProductClicked!=null&&lastProductClicked!='undefined')
RestoreOldQuantity("txtOpenListQty_",lastProductClicked)}
lastProductClicked=sProductID;RemoveUpdateDiv();var oAproveBox=oObjectClicked;oAproveBox.attr("class","OpenListProduct OpenListProductClicked");oAproveBox.find("span[class^='QAprove']").css("display","inline");$(".miniBasketQClicked span[class^='QAprove']").css("display","none");$(".miniBasketQClicked").attr("class","miniBasketQ");var oAproveBox=oObjectClicked;oAproveBox.find("span[class^='QAproveMiddle']").unbind("click");oAproveBox.find("span[class^='QAproveMiddle']").bind("click",function(event){UpdateOpenListBasketItem(sProductID);});CloseQtyModule();oBoxX=oAproveBox.offset().left;oBoxY=oAproveBox.offset().top;$(document).bind("keypress",function(event){if(event.keyCode=='27'){event.preventDefault();RestoreOldQuantity("txtOpenListQty_",sProductID)
oAproveBox.attr("class","OpenListProduct");oAproveBox.find("span[class^='QAprove']").css("display","none");oAproveBox.find("span[class^='QAproveMiddle']").unbind("click");oAproveBox.find("span[class^='QAproveMiddle']").bind("click",function(event){UpdateOpenListBasketItem(sProductID);});if(isIPad()){$("span[class^='QAproveClose']").unbind("click");$("span[class^='QAproveClose']").bind("click",function(e){$(".miniBasketQClicked span[class^='QAprove']").css("display","none");$(".miniBasketQClicked").attr("class","miniBasketQ");e.stopPropagation();});}}});$(document).bind("click",function(event){event.stopPropagation();var iXAdd=74;var iYAdd=56;if(event!=null&&event.currentTarget!=null&&event.currentTarget.activeElement!=null&&event.currentTarget.activeElement.id!=null&&event!='undefined'&&event.currentTarget!='undefined'&&event.currentTarget.activeElement!='undefined'&&event.currentTarget.activeElement.id!='undefined'){if(event.currentTarget.activeElement.id.toLowerCase().indexOf("ddlumextbasket")!=-1)
return;}
if((event.pageX<oBoxX||event.pageX>oBoxX + iXAdd)&&(event.pageY<oBoxY||event.pageY>oBoxY + iYAdd)){RestoreOldQuantity("txtOpenListQty_",sProductID)
oAproveBox.attr("class","OpenListProduct");oAproveBox.find("span[class^='QAprove']").css("display","none");oAproveBox.find("span[class^='QAproveMiddle']").unbind("click");}});}
function RemoveUpdateDiv(){$(".OpenListProductClicked span[class^='QAprove']").css("display","none");$(".OpenListProductClicked").attr("class","OpenListProduct");}
function UpdateOpenListBasketItem(iProdID){ShowGeneralLoadingPanel();$("div[id$=radGeneralLoad]").css("z-index","998");$("div[class^='generalPageMask']").css("z-index","998");$("div[class^='divOpenList']").css("z-index","997");var oProdQty=$("div[class^='OpenListProductOver'][pid='"+ iProdID +"'] div.openlistq input.BasketProductQuantityTextBox").val();var remarks=$('#txtOpenList_'+ iProdID).val()
var oOldQuantity="0";if($("div[class^='OpenListProductOver'][pid='"+ iProdID +"']").length>0)
oOldQuantity=$("div[class^='OpenListProductOver'][pid='"+ iProdID +"']").attr("oldqty");if(!ValidateMaxQty(oProdQty,ITEM_MAX_QTY)){$("#txtOpenListQty_"+ iProdID).val(oOldQuantity);InvertIndex();HideGeneralLoadingPanel();return false;}
if(oProdQty==0){var catNo=$("div[class^='OpenListProductOver'][pid='"+ iProdID +"']").attr("catno");RemoveProductFromOpenList(iProdID,catNo,false);RefreshOpenListFooter();HideGeneralLoadingPanel();}
else{var unitofmeasure=GetUnitOfMeasureById('#ddlUMExtBasket_'+ iProdID,'#divUMExtBasket_'+ iProdID);$.post(GetCurrentPageUrlString(),{AjaxCallAction:"miniupdateitem",BasketItemID:iProdID,ItemQuantity:oProdQty,ItemOldQuantity:oOldQuantity,ItemUM:unitofmeasure,ItemRemarks:remarks},function(data){if(data.indexOf("SessionExpired")==0){DoSessionExpired();return;}
else{if(data.indexOf('שגיאהכמות:')==0){ShowLimitedAmountsPopup();HideGeneralLoadingPanel();HideBasketDetailsLoadingPanel();return;}
if(data.indexOf('שגיאה: ')==0){HideGeneralLoadingPanel();ShowErrMsg('שגיאה',data.replace('שגיאה: ',''));return;}
var oAprovalBox=$("div[class^='OpenListProductOver'][pid='"+ iProdID +"'] div.OpenListProduct");if(oAprovalBox.length==0){oAprovalBox=$("div[class^='OpenListProductRow'][pid='"+ iProdID +"'] div.OpenListProduct");}
oAprovalBox.find("span[class^='QAprove']").css("display","none");oAprovalBox.attr("class","miniBasketQ miniBasketQUpdated");$("<span class='check'></span><span class='updatedMessage'>הפריט המעודכן</span>").appendTo(oAprovalBox);oAprovalBox.parent().find("div.OpenListTitleRow").css("margin-right","10px");setTimeout("RestoreOpenList();",2000);RefreshDetailedBasket('divOpenListContent');if(CheckDisplayModeThumb()){ProductActionHighlight(iProdID,'עדכון הכמות בוצע');}
else{ProductLineActionHighlight(iProdID);}
RefreshOpenListFooter();if(!IsExtended()){HideGeneralLoadingPanel();}
else{InvertIndex();HideGeneralLoadingPanel();}}});}
ExtendedBannerPromo();return false;}
function RestoreOpenList(){var oQBox=$("div.miniBasketQUpdated");oQBox.find("span.check").remove();oQBox.find("span.updatedMessage").remove();oQBox.attr("class","OpenListProduct");oQBox.parent().find(".OpenListTitleRow").css("margin-right","0px");}
