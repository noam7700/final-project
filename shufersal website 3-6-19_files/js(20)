function ValidateMaxQty(qty,maxqty){maxqty=maxqty||ITEM_MAX_QTY;if(0 + qty>maxqty){ShowErrMsg('הודעה','.'+' כמות מקסימלית לפריט זה היא '+ String(maxqty));return false;}
return true;}
