else{if(event.srcElement.id=='ctl00_PlaceHolderMain_ucMain_OrderPageFood_ucOrderDeliveryAddress_ucDeliveryWindowCombo_ddlCollectionTime_Input'||event.srcElement.id=='ctl00_PlaceHolderMain_ucMain_OrderPageFood_ucOrderDeliveryAddress_ucDeliveryWindowCombo_ddlCollectionTime_Arrow')GTMSupplyWindowsCheckout_Open();}
