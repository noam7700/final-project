function GeneralIncreaseBoxQuantity(inputQtyID,ddlUM,divUM,prodID){var Qty=$("input[id='"+ inputQtyID + prodID +"']");var dQuantity=parseFloatFixed(Qty.val());var increase=parseFloat(Qty.attr('iq'));if(isNaN(increase))
increase=1;if(!ValidateMaxQty(dQuantity + increase,GetMaxQtyByUM(GetUnitOfMeasureById('#'+ ddlUM + prodID,'#'+ divUM + prodID))))return;var myval=0 + dQuantity + increase;Qty.val(myval.toFixed(2));}
function GeneralDecreaseBoxQuantity(inputQtyID,prodID){var Qty=$("input[id='"+ inputQtyID + prodID +"']");var dQuantity=parseFloatFixed(Qty.val());var increase=parseFloat(Qty.attr('iq'));if(isNaN(increase))
increase=1;var myval=0 + dQuantity-increase;if(myval>0)
Qty.val(myval.toFixed(2));}
function ChangeTextBoxQuantity(inputQtyID,ddlUM,divUM,prodID){var Qty=$("input[id='"+ inputQtyID + prodID +"']");var dQty=parseFloatFixed(Qty.val());var increase=parseFloat(Qty.attr('iq'));if(isNaN(increase))
increase=1;var myval=0 + parseFloat(dQty);var unitMeasure=GetUnitOfMeasureById('#'+ ddlUM + prodID,'#'+ divUM + prodID);if(!ValidateMaxQty(myval,GetMaxQtyByUM(unitMeasure)))
myval=increase;if(dQty>0&&((dQty*100)%(increase*100)==0||unitMeasure.charAt(0)!='י')){Qty.val(myval);}
else{ShowErrMsg('שגיאה','כמות צריכה להיות חיובית ובמספרים שלמים '+ increase);Qty.val(increase);}}
