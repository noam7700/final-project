else{if(data.indexOf('שגיאה: ')==0){ShowErrMsg('שגיאה',data.replace('שגיאה: ',''));return;}
if(jQuery(promotionWrapper).attr("IsValidated")!="1"){jQuery(promotionWrapper).attr("IsValidated","1");switch(data){case"OK":break;case"Info":break;default:return;break;}}
m_CurrentPromoObject=promotionWrapper;if($(QtyControl).attr("isupdate")=="1"){ProductOfferUpdateToBasket(prodID,$(QtyControl).attr("oldqty"),quantity,UnitOfMeasure);}
else{if(quantity>0){ProductOfferAddToBasket(prodID,quantity,UnitOfMeasure);}
else{ShowErrMsg("Error","Please select a valid quantity");}}}});}
else{ShowErrMsg("Error","Please provide all values for adding product!");}}
