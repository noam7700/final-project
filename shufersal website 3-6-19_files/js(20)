function ChangeQ(oID,oQty){if(oQty!=null){var oBasketDiv=$('#dvMinibasketContent');var oProdQtyCtrl=oBasketDiv.find("div[class^='productRow'][pid='"+ oID +"']").find("input.number");var oProdQty=parseFloatFixed(oProdQtyCtrl.val());var increment=parseFloat(oProdQtyCtrl.attr('iq'));if(isNaN(increment))increment=1;if(oQty==1)
oProdQty=oProdQty + increment;if(oQty==-1)
oProdQty=oProdQty-increment;if(oProdQty<=0){oProdQty=0;}
if(!ValidateMaxQty(oProdQty,GetMaxQtyByUM(GetUnitOfMeasureById('#ddlUMminibasket_'+ oID,'#divUMminibasket_'+ oID))))return;oProdQtyCtrl.val(oProdQty.toFixed(2));}}
function CalculateFooterTotalPrice(){var totalPrice=0;$('#dvMinibasketContent').find(".productRow .productPrice").each(function(iIndex,oElem){var boxPrice=parseFloat($(oElem).html());if(boxPrice!=null&&boxPrice!='undefined'&&boxPrice!=''){totalPrice +=parseFloat(boxPrice);}});$("#miniBasketpayAmount").html("&#8362; "+ addCommas(parseFloat(totalPrice).toFixed(2)));}
function AddToBasketQtyChange(prodID,oQty,maxQty){var OrderBcontent=$("div[id$='htmlContent']");var Qty=OrderBcontent.find("input[id=qtyboxlistminibasket_"+ prodID +"]");var dQuantity=parseFloatFixed(Qty.val());var increment=parseFloat(Qty.attr('iq'));if(isNaN(increment))increment=1;if(oQty==1)
dQuantity=dQuantity + increment;if(oQty==-1)
dQuantity=dQuantity-increment;if(dQuantity<=0){dQuantity=0;}
if(!ValidateMaxQty(dQuantity,maxQty))return;Qty.val(dQuantity.toFixed(2));}
function AddToBasketListQtyChange(prodID,oQty,maxQty){var OrderBcontent=$("div[id$='htmlContent']");var Qty=OrderBcontent.find("input[id=qtyboxlistminibasket_"+ prodID +"]");var dQuantity=parseFloatFixed(Qty.val());var increment=parseFloat(Qty.attr('iq'));if(isNaN(increment))increment=1;if(oQty==1)
dQuantity=dQuantity + increment;if(oQty==-1)
dQuantity=dQuantity-increment;if(dQuantity<=0){dQuantity=0;}
if(!ValidateMaxQty(dQuantity,maxQty))return;Qty.val(dQuantity.toFixed(2));}
function AddToBasketListQtyDiners(prodID,oQty,maxQty){var OrderBcontent=$("div[id$='htmlContent']");var Qty=OrderBcontent.find("input[id=qtyboxlistminibasket_"+ prodID +"]");if(!ValidateMaxQty(oQty,maxQty))return;Qty.val(oQty.toFixed(2));}
function ExtendedBasketOnQtyChange(prodID){var Qty=$("#txtOpenListQty_"+ prodID);var dQuantity=parseFloatFixed(Qty.val());var increase=parseFloat(Qty.attr('iq'));if(isNaN(increase))
increase=1;var unitMeasure=GetUnitOfMeasureById('#ddlUMExtBasket_'+ prodID,'#divUMExtBasket_'+ prodID);if(dQuantity>0&&(dQuantity%increase==0||unitMeasure.charAt(0)!='י')){if(!ValidateMaxQty(dQuantity,unitMeasure))return;Qty.val(dQuantity);}
else{ShowErrMsg("","יש להזין אך ורק כמות פריטים חיובית");oAproveBox=$('#OpenListProduct_'+ prodID);oAproveBox.find("span[class^='QAproveMiddle']").unbind("click");if(Qty.attr('oldqty')!='undefined'&&Qty.attr('oldqty')!=null)
RestoreOldQuantity('txtOpenListQty_',prodID);else
Qty.val(increase);}}
function ProdLineOnQtyChange(prodID){var Qty=$("tr[grdline_rowprodid='"+ prodID +"'] #prodlineqtybox_"+ prodID);var dQuantity=parseFloatFixed(Qty.val());var oTXT=$("#prodlineqtybox_"+ prodID);var increase=parseFloat(oTXT.attr('iq'));if(isNaN(increase))
increase=1;var unitMeasure=GetUnitOfMeasureById('#ddlUMLineView_'+ prodID,'#divUMLineView_'+ prodID);if(dQuantity>0&&((dQuantity*100)%(increase*100))==0||unitMeasure.charAt(0)!='י'){if(!ValidateMaxQty(dQuantity,GetMaxQtyByUM(unitMeasure)))return;oTXT.val(dQuantity.toFixed(2));}
else{ShowErrMsg('שגיאה','כמות צריכה להיות חיובית ובמספרים שלמים '+ increase);oTXT.val(increase);}}
function ChangeQOnChange(oID,oQty,maxQty){if(oQty!=null&&oQty!='undefined'&&oQty!=''){var oProdQtyCtrl=$("div[class^='productRow'][pid='"+ oID +"']").find("input.number");var increment=parseFloat(oProdQtyCtrl.attr('iq'));if(isNaN(increment))increment=1;var unitMeasure=GetUnitOfMeasureById('#ddlUMminibasket_'+ oID,'#divUMminibasket_'+ oID);if(oQty>0&&(oQty%increment==0||unitMeasure.charAt(0)!='י')){if(!ValidateMaxQty(oQty,maxQty)){$("#qtyboxlistminibasket_"+ oID).val(maxQty);return;}
oProdQtyCtrl.val(oQty);$("input[id='ProdUpdateQuantityTextBox_"+ oID +"']").val(oQty);$("tr[grdline_rowprodid='"+ oID +"'] #prodlineqtybox_"+ oID).val(oQty);}
else{ShowErrMsg("","יש להזין אך ורק כמות פריטים חיובית");oProdQtyCtrl.val(increment);}}}
