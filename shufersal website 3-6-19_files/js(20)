function DragAndDropProduct(oProdDiv){if(Ie7()){oProdDiv.draggable({appendTo:'body'})}
oProdDiv.draggable({helper:'clone',revert:'invalid',containment:'document',start:function(event,ui){var iProductId=$(ui.helper).attr('DdPiD');ui.helper.find("#divProductAddToBasket_"+ iProductId).remove();ui.helper.find('#divProductQuantity_'+ iProductId).remove();ui.helper.find("#divProductDetails").css("height","100px");if(Ie7()||Ie8())
ui.helper.css("height","105px");else
if(oProdDiv.hasClass("PWdivProductNF")){ui.helper.css("height","320px");}else{ui.helper.css("height","188px");}
if(Ie7()||Ie8()){ui.helper.find('#divProductImage_'+ iProductId).remove();ui.helper.css({"border":"1px solid","z-index":"9999"});ui.helper.find("#divProductDetails").css("background-image","none");ui.helper.find(".PWSpecialOffer").remove();}
else{if(oProdDiv.hasClass("PWdivProductNF")){ui.helper.css({"background-image":"url(/_layouts/images/Shufersal/Images/Drag_box_Large.png)","background-repeat":"no-repeat","z-index":"9999"});}else{ui.helper.css({"background-image":"url(/_layouts/images/Shufersal/Images/Drag_box.png)","background-repeat":"no-repeat","z-index":"9999"});}}
ui.helper.find("div.PWSpecialOffer").removeAttr("onmouseover").removeAttr("onmouseout");m_IgnoreHoverProduct=iProductId;ui.helper.find("#divProductDetails").css("background-color","#ffffff");$("#DragDropHoleDiv").css("display","inline");var height;if(isHD){height=$('#dvHDbasketContent').height();}else{height=$('#dvMinibasketContent').height();}
if(IsIe())
height-=17;else
height-=14;$("#DragDropHoleDiv").css("height",height +'px');if(IsIe()){$("#dvMinibasketContent").css("background-position","top right");$("#DragDropHoleDiv").css("left","16px")}},stop:function(){m_IgnoreHoverProduct="";$("#DragDropHoleDiv").css("display","none");if(IsIe()){$("#dvMinibasketContent").css("background-position","");}}});var dropDiv
if(isHD){dropDiv=$("#dvHDbasketContent");}else{dropDiv=$("#dvMinibasketContent");}
dropDiv.droppable({drop:function(event,ui){$("#dvMinibasketContent").attr("class","Float miniBasket");var iProductId=$(ui.draggable).attr('DdPiD');if(ExistsProductInBasket(iProductId)){var oQty=$('#dvMinibasketContent').find("div[class^='productRow'][pid='"+ iProductId +"'] input.number");var catalogQty=$("input[id='ProdUpdateQuantityTextBox_"+ iProductId +"']");var dQuantity=parseFloatFixed(catalogQty.val());if(oQty.length>0){if(dQuantity==parseFloatFixed(oQty.val())){ChangeQ(iProductId,1);UpdateProductToBasket(iProductId);iqu(iProductId);}
else{ChangeQFixedQuantity(iProductId,dQuantity);UpdateProductToBasket(iProductId);}}}
else{pcadd(iProductId);}}});}
