function DoUpdateProductToBasket(iProdID,oldqty,qty,remarks,unitofmeasure,updateProductToBasketCallback){if(isHD){var x=document.getElementById("basketFrame");var ss=x.contentWindow.angular.element(x.contentWindow.document.querySelector('[ng-controller=TypeaheadCtrl]')).scope();ss.updateProductById(iProdID,qty,remarks)
$('.miniBasketFooter').addClass('loadFooter');}else{ShowBasketLoadingPanel();}
if(qty==0){MiniBasketRemoveProduct(iProdID,null);}
else{if(typeof(oldqty)=='undefined')
oldqty="1";$.post(GetCurrentPageUrlString(),{AjaxCallAction:"miniupdateitem",BasketItemID:iProdID,ItemQuantity:qty,ItemOldQuantity:oldqty,ItemRemarks:remarks,ItemUM:unitofmeasure},function(data){if(data.indexOf("SessionExpired")==0){DoSessionExpired();return;}
else{if(data.indexOf('שגיאהכמות:')==0){ShowLimitedAmountsPopup();HideBasketLoadingPanel();HideBasketDetailsLoadingPanel();return;}
HideBasketLoadingPanel();if(data.indexOf('שגיאה: ')==0){ShowErrMsg('שגיאה',data.replace('שגיאה: ',''));return;}
if(typeof(updateProductToBasketCallback)=='function')
if(isHD){$('.loadFooter').removeClass('loadFooter');}
updateProductToBasketCallback(data,iProdID,qty);}});}
return false;}
