function ReplaceClassName(iCurrent,isSelect){var sIDLeft,sIDRight,sIDCenter;sIDLeft="item"+ iCurrent +"Left";sIDRight="item"+ iCurrent +"Right";sIDCenter="item"+ iCurrent +"Center";if(document.getElementById(sIDLeft)!=null){if(isSelect){document.getElementById(sIDLeft).className=document.getElementById(sIDLeft).className.replace("Selected","");}
