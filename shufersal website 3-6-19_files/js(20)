function GetCategoryLevel(cid){if($("div.ci[itemid='"+ cid +"']").length==1)return 1;if($("div.scc a[lv2cat='"+ cid +"']",".BackgroundMenu").length==1)return 2;if($("div.scc a[lv3cat='"+ cid +"']",".BackgroundMenu").length==1)return 3;return 0;}
