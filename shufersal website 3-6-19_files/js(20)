else{$(".miniBasketQClicked span[class^='QAprove']").css("display","none");$(".miniBasketQClicked").attr("class","miniBasketQ");}
e.stopPropagation();});function ddlBranch_OnClientLoad(sender){var input=sender.get_inputDomElement();input.readOnly="readonly";}
function isIPad(){if(navigator.userAgent.match(/Android/i)||navigator.userAgent.match(/iPad/i)||navigator.userAgent.match(/iPod/i)){tocheModeFlag=true;return true;}}
