else{filter.attr('mode','0');filterList.attr('mode','0');filterPromo.attr('mode','0');m_ShowOnlyDeals=showOnlyDeals='0';m_ShowOnlyDealsflag=true;}
if(!m_ShowOnlyDealsflag)
FillProducts(m_sSelectedCategory,m_sCurrentDisplayLevel,'0',showOnlyDeals,'');else ShowMutag();ChangeDealsButtonText();}
