function isValidEmailAddress(emailAddress){var pattern=new RegExp(/^(("[\w-\s]+")|([\w-]+(?:\.[\w-]+)*)|("[\w-\s]+")([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/i);return pattern.test(emailAddress);};function ValidateNumericExactLength(text,number){var pattern=new RegExp(/^[0-9]{4}$/);return pattern.test(text);};function ValidateNumericMaxLength(text,number){var pattern=new RegExp(/^[0-9]{0,10}$/);return pattern.test(text);};function ResetTextBoxDefaultValue(defaultText,controlID,isTelerikbox){if(isTelerikbox){if($find(controlID).get_value()==defaultText)
$find(controlID).set_value("");}
else{if($("input[id$='"+ controlID +"']").val()==defaultText)
$("input[id$='"+ controlID +"']").val("");}}
