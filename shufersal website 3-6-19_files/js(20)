else{var footertotalitems,footertotalsavings,footertotalamount;var oaData=data.split(',');footertotalitems=oaData[0];footertotalsavings=oaData[1].replace("-","");footertotalamount=oaData[2];var ShowHealtierProductLeaf=oaData[3];if(footertotalitems==null||footertotalitems=='undefined'||footertotalitems==''){footertotalitems=0;}
if(footertotalsavings==null||footertotalsavings=='undefined'||footertotalsavings==''){footertotalsavings=0;}
if(footertotalamount==null||footertotalamount=='undefined'||footertotalamount==''){footertotalamount=0;}
m_footertotalamount=footertotalamount;var oLists=$("div[id$='_divMiniBasketShopperLists']");oLists.css("display","inline");$("#miniBasketItemsNo").html("סה\"כ "+ footertotalitems +" מוצרים ");if(footertotalitems>0){$("#miniBasketBox").find(".miniBasketOptions .delete").show();$("#miniBasketBox").find(".miniBasketOptions .OpenList").show();}
