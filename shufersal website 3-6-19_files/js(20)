return;var oBasketDiv=$('#dvMinibasketContent');var oCategFromBasket=oBasketDiv.find("div.miniBasketCategory[cid='"+ iCategoryID +"']");if(oCategFromBasket.length>0){scrollWinIntoView(oCategFromBasket,oBasketDiv,instantScroll);}}
function MiniAddRowsToScrollIntoView(valueToScroll){var minibasket=$('#dvMinibasketContent');var maxHeight=minibasket.height();var h=minibasket.height();var y=minibasket.scrollTop();var numberOfRowsToAdd=0;var scrollHeight=document.getElementById("dvMinibasketContent").scrollHeight;var htemp=scrollHeight-h;if(parseFloat(valueToScroll)+ parseFloat(y)>parseFloat(htemp)){var scrollDifference=parseInt(valueToScroll)+ parseInt(y)-parseInt(htemp);numberOfRowsToAdd=parseInt(scrollDifference/37)+ 1;}
for(var index=0;index<numberOfRowsToAdd;index++){minibasket.append("<div class='productRowEmpty' id='productRowEmpty'></div>");}}
