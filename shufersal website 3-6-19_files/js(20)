function btnSaveToList_onclick(){var cbSaveToNewList=$('input[id=rcbSaveToL]:checked');var cbSaveToExistingList=$('input[id=rcbSaveToEL]:checked');var msg=jQuery('div[id=SaveToListVal]');var sListName="";var sAjaxCallAction="";if((cbSaveToNewList.length<=0)&&(cbSaveToExistingList.length<=0)){msg.css("display","block");}
else{var msg=jQuery('div[id=SaveToListVal]');msg.css("display","none");if(cbSaveToNewList.length>0){sListName=$('input[id$=txtSaveList]').val();if(sListName=="הקלד את שם הרשימה"||sListName.length==0){msg.css('display','inline');return;}
else
sAjaxCallAction="OrderAddToList";}
else if(cbSaveToExistingList.length>0){sListName=$('input[id$=ddlCustomerLists_Input]').val();if((sListName!="בחר...")&&(sListName!=null))
sAjaxCallAction="OrderUpdateToList";else{msg.css('display','inline');return;}}
ShowGeneralLoadingPanel();HideSaveToListPopup();$.post(GetCurrentPageUrlString(),{AjaxCallAction:sAjaxCallAction,ListName:sListName},function(data){if(data.indexOf("SessionExpired")==0){DoSessionExpired();return;}
else{if(data.indexOf('שגיאה: ')==0){if(data.indexOf('כמות:')>-1){var index=data.indexOf('כמות:');ShowSaveToListErrorPopup(data.substring(index + 5));if($('input[id=rcbSaveToEL]:checked').length>0)
WT_SaveBasketToList('add','failed',data);else if($('input[id=rcbSaveToL]:checked').length>0)
WT_SaveBasketToList('save','failed',data);}
else{ShowSaveToListErrorPopup();if($('input[id=rcbSaveToEL]:checked').length>0)
WT_SaveBasketToList('add','failed','רשימה לא נשמרה');else if($('input[id=rcbSaveToL]:checked').length>0)
WT_SaveBasketToList('save','failed','רשימה לא נשמרה');}
HideGeneralLoadingPanel();return;}
else{if(cbSaveToNewList.length<=0&&cbSaveToExistingList.length<=0){ShowSaveToListErrorPopup();}
else{ShowSaveToListConfirmationPopup();if($('input[id=rcbSaveToEL]:checked').length>0)
WT_SaveBasketToList('add','success');else if($('input[id=rcbSaveToL]:checked').length>0)
WT_SaveBasketToList('save','success');}}
HideGeneralLoadingPanel();}});}}
function UpdatePasswordSubmit(){var oPwdCtrl=$('input[id$=txtOldPasswordConfirm]');var oNewPwdCtrl=$('input[id$=txtNewPassword]');var oNewConfPwdCtrl=$('input[id$=txtConfirmNewPassword]');var oPwd=oPwdCtrl.val();var oNewPwd=oNewPwdCtrl.val();var oNewConfPwd=oNewConfPwdCtrl.val();$('#msgUpdatePwdResult').html("");jQuery("#pwdconfirmerrmsg").html("");if(oPwd.length==0){jQuery("#pwdconfirmerrmsg").html("שדה חובה");return;}
jQuery("#pwdnewerrmsg").html("");if(oNewPwd.length==0){jQuery("#pwdnewerrmsg").html("שדה חובה");return;}
jQuery("#pwdconfirmnewerrmsg").html("");if(oNewConfPwd.length==0){jQuery("#pwdconfirmnewerrmsg").html("שדה חובה");return;}
var oVldExpr=oPwdCtrl.attr("VldExpr");var re=new RegExp(oVldExpr);if(!oPwd.match(re)){jQuery("#pwdconfirmerrmsg").html(oPwdCtrl.attr("VldExprMessage"));return false;}else{jQuery("#pwdconfirmerrmsg").html("");}
oPwdCtrl.keyup(function(){jQuery("#pwdconfirmerrmsg").html("");});oVldExpr=oNewPwdCtrl.attr("VldExpr");var re=new RegExp(oVldExpr);if(!oNewPwd.match(re)){jQuery("#pwdnewerrmsg").html(oNewPwdCtrl.attr("VldExprMessage"));return false;}else{jQuery("#pwdnewerrmsg").html("");}
oNewPwdCtrl.keyup(function(){jQuery("#pwdconfirmerrmsg").html("");});oVldExpr=oNewConfPwdCtrl.attr("VldExpr");var re=new RegExp(oVldExpr);if(!oNewConfPwd.match(re)){jQuery("#pwdconfirmnewerrmsg").html(oNewConfPwdCtrl.attr("VldExprMessage"));return false;}else{jQuery("#pwdconfirmnewerrmsg").html("");}
oNewConfPwdCtrl.keyup(function(){jQuery("#pwdconfirmnewerrmsg").html("");});if(oNewPwd!=oNewConfPwd){jQuery("#pwdconfirmnewerrmsg").html("אין התאמה בין הסיסמאות");return false;}
else{jQuery("#pwdconfirmnewerrmsg").html("");}
ShowPwdLoadingPanel();var frameContents=$('iframe[id$=frmChangePwd]').contents();frameContents.find('#hdnPass').val($('#popoupBoxContent').find('input[id$=txtOldPasswordConfirm]').val());frameContents.find('#hdnNewPass').val($('#popoupBoxContent').find('input[id$=txtNewPassword]').val());frameContents.find('#hdnNewPassConfirm').val($('#popoupBoxContent').find('input[id$=txtConfirmNewPassword]').val());var submitButton=frameContents.find('#submitBtn');submitButton.click();return false;}
function ChangePwdCallback(data){HidePwdLoadingPanel();if(data.indexOf('שגיאה: ')==0){ShowErrMsg('שגיאה',data.replace('שגיאה: ',''));return;}
if(data=='true'){PopupClose();}
else{if(data=='incorrect')
