function ValidateCaptcha(sIDCaptcha,sMainWrapperCSS){var input=$(sMainWrapperCSS).find("input[id$='"+ sIDCaptcha +"']").val();$("#lblReqCaptcha").css("display","none");$("#lblErrorCaptcha").css("display","none");if(input==""){$("#lblReqCaptcha").css("display","block");return false;}
var id=$(sMainWrapperCSS).find("img[id$='ShufersalCaptchaImage']").attr("guid");var dataToSend="v="+ escape(input)+"&id="+ id;var sResult=$.ajax({type:"GET",data:dataToSend,url:"/_layouts/Shufersal_Pages/ValidateCapcha.aspx",contentType:"application/x-www-form-urlencoded",async:false}).responseText;if(sResult=="false"){DoCaptchaRefresh(true);return false;}
return true;}
function ValidateContactUsForm(oElement){var oMainDiv=$(".contactwrapper");if(oMainDiv.length>0){oMainDiv.find(oElement).each(function(iIndex,oElem){var txtID=oElem.id;var sTextVal='';if(txtID.indexOf("ClientState")==-1&&txtID.indexOf("_text")==-1&&txtID.indexOf("txtCode")==-1&&txtID.indexOf("ucShufCaptcha")==-1){var lblID=txtID.replace("txt","lblError");var lblElement=$("#"+ lblID);sTextVal=$(oElement +'[id$='+ txtID +'_text]').val()
if(lblElement.length>0){if(sTextVal.length==0){lblElement.css("display","inline");lblElement.html(lblElement.attr("ReqMsg"));bContactValid=false;}
else{if(!ValidateAlphaNumericMaxLength(sTextVal)){lblElement.css("display","inline");lblElement.html(lblElement.attr("InvalidMsg"));bContactValid=false;}
else{lblElement.css("display","none");lblElement[0].innerText="";}}}}});}}
