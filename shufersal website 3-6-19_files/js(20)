function CollectOrderNewPaymentData(){var val='';val +="CardType="+ $("input:radio[name$=CreditCardType]:checked").val();val +="&CardNum="+ $("input[id$=_txtCardNumber]").val();val +="&ValidDate="+ $find(getId("_ddlMonth"))._value + $find(getId("_ddlYear"))._value;if($("input[id$=_txtCVV]").length>0)
val +="&CVV="+ $("input[id$=_txtCVV]").val();var iCardOwner=$("input:radio[name$=rblCardHolder]:checked").val();val +="&CardOwner="+ iCardOwner;val +="&CardHolder="+ iCardOwner;if(iCardOwner==2){val +="&HolderName="+ $("input[id$=_txtOtherPersonName]").val();val +="&HolderID="+ FillIDLeadingZeros($("input[id$=_txtOtherPersonID]").val());val +="&HolderPhone="+ $find(getId("ddlOtherPhoneHome"))._value + $("input[id$=_txtOtherPhoneHome]").val();val +="&HolderCity="+ $find(getId("txtOtherPersonLocality"))._value;val +="&HolderStreet="+ $find(getId("txtOtherPersonStreet"))._value;val +="&HolderHouseNum="+ $("input[id$=_txtHouseNo]").val();val +="&HolderAppartment="+ $("input[id$=_txtApartment]").val();}
else{val +=$("input[id$=hdnOwnerAddress]").val();}
if($find(getId("txtCardNumber")).get_enabled())
val +='&ActionType=1';else
val +='&ActionType=2';var ChkDefault=$("input[id$=_chkDefault]");var DefaultChecked=false;if(ChkDefault.length>0){DefaultChecked=ChkDefault[0].checked;}
else{DefaultChecked=true;}
var PaymentType="0";if(DefaultChecked)
PaymentType="1";val +="&PaymentType="+ PaymentType;return val;}
function OnOrderSaveCreditCardInfo(){var bResponse=false;var dataToSend=CollectOrderNewPaymentData();dataToSend +='&WSActionType=Update';dataToSend +='&CLINumber='+ $('input[id$=hdnCLINumber]').val();dataToSend +='&UserName='+ $('input[id$=hdnUserName]').val();dataToSend +='&RegistrationStatus=2';var oCall=AddActionForCCHandler();if(oCall.ActionId==0||oCall.SessionId==0)
return false;dataToSend +='&ActionID='+ oCall.ActionId +'&SessionID='+ oCall.SessionId;ShowCCLoadingPanel();CallCCHandlerService(OnCreditCardAdd,OnCreditCardAdd,dataToSend,GetCreditCardHandlerUrl());return false;}
function CallCCHandlerService(successFunction,faliureFunction,dataToSend,cchUrl){$.ajax({type:'POST',url:cchUrl,data:dataToSend,contentType:"application/x-www-form-urlencoded",dataType:'jsonp',jsonp:'jsonp_callback',async:false,success:function(msg){successFunction(msg,true);},error:function(msg){faliureFunction(msg,false);}});}
function OnCreditCardAdd(msg,bSuccess){if(bSuccess){switch(msg.response){case 0:ShowErrMsg('שגיאה',ErrorMessageForCreditCards(msg.errcode));break;case 1:JSPopuplateCC(msg.response);break;default:ShowErrMsg('שגיאה','אירעה שגיאה בעדכון כרטיס אשראי');break;}}
else{ShowErrMsg('שגיאה',ErrorMessageForCreditCards(msg.errcode));}
HideCCLoadingPanel();}
function OnAccountCreditCardAdd(msg,bSuccess){HideCCLoadingPanel();if(bSuccess){switch(msg.response){case 0:ShowErrMsg('שגיאה',ErrorMessageForCreditCards(msg.errcode));break;case 1:JSPopuplateAccountCC(msg.response);break;default:ShowErrMsg('שגיאה','אירעה שגיאה בעדכון כרטיס אשראי');break;}}
else{ShowErrMsg('שגיאה','אירעה שגיאה בעדכון כרטיס אשראי');}}
function ErrorMessageForCreditCards(errorCode){var errorMessage='אירעה שגיאה בעדכון כרטיס אשראי';if(errorCode!=null&&errorCode!='undefined'){switch(errorCode){case'3004':errorMessage='כרטיס אשראי כבר קיים';break;default:break;}}
return errorMessage;}
