function ListAddtoBasket(){var qtys=[],oldqtys=[],prodIDs=[],inbasket=[],unitofmeasures=[],tmp,remarks=[];jQuery.each(jQuery('DIV.OrderBasketcCkbV',"DIV.OrderBasketContainer"),function(index,domElem){var sendClearBasket=false;if(document.getElementById('ctl00_PlaceHolderMain_ucPopupMasterLoader_ucListAddToBasketPopup_hdClearBasket')&&document.getElementById('ctl00_PlaceHolderMain_ucPopupMasterLoader_ucListAddToBasketPopup_hdClearBasket')!='undefined'){var clearBasket=document.getElementById('ctl00_PlaceHolderMain_ucPopupMasterLoader_ucListAddToBasketPopup_hdClearBasket').value;if(clearBasket=='true'){sendClearBasket=true;}}
if($(this).attr("productinbasket")!='true'||sendClearBasket){var oElem=$(domElem).parent();var pid=oElem.attr("pid");var iQty=GetValueNotNull($(oElem).find("input[id='qtyboxlistminibasket_"+ pid +"']").val(),0);var iOldQty=GetValueNotNull($(oElem).find("input[id='qtyboxlistminibasket_"+ pid +"']").attr("oldqty"),'1');var rem=$(domElem).parent().attr("re");if(rem!="")
remarks.push(rem);else
remarks.push(" ");if(iQty>ITEM_MAX_QTY){var prodmaxqty=GetMaxQtyByUM(GetUnitOfMeasureById('#ddlUMlistminibasket_'+ pid,'#divUMlistminibasket_'+ pid));if(iQty>prodmaxqty){qtys.push(prodmaxqty);}
else{qtys.push(iQty);}}
else
qtys.push(iQty);oldqtys.push(iOldQty);prodIDs.push(pid);inbasket.push(GetValueNotNull($(oElem).attr("isinbasket"),'0'));unitofmeasures.push(GetListUM(pid));}
if(isHD){document.getElementById("basketFrame").contentDocument.location.reload(true);}});if(listTypeWT){WT_List_AddPrdctsFromList(listTypeWT);}
return RangeAddUpdateProdsInBasket(prodIDs,qtys,oldqtys,unitofmeasures,inbasket,remarks,false);}
function UpdateList(previousShopperListName){var sListName=jQuery("input.listnametextbox").val();var sCategoryName="";var sQuantity="";var sUm="";var sProductId="";var pid="";var semarks="";var addremark="";jQuery.each(jQuery('DIV.ProducRow',"DIV.OrderBasketContainer"),function(index,domElem){var oElem=$(domElem).parent();sCategoryName=sCategoryName + oElem.attr("cid")+",";pid=jQuery(domElem).attr("pid");sQuantity=sQuantity + GetValueNotNull(jQuery(domElem).find("input[id='qtyboxlistminibasket_"+ pid +"']").val(),0)+",";sProductId=sProductId + pid +",";sRemarks=$(domElem).attr("re");if(sRemarks.length>0)
addremark=addremark + sRemarks +"_";else
addremark=addremark +" "+"_";sUm=sUm + GetListUM(pid)+",";});$.post(GetCurrentPageUrlString(),{AjaxCallAction:"updatelistitems",sPreviousShopperListName:previousShopperListName,sListName:sListName,sCategoryName:sCategoryName,sQuantity:sQuantity,addremark:addremark,sUm:sUm,sProductId:sProductId},function(data){if(data.indexOf("SessionExpired")==0){DoSessionExpired();return;}
