else{HideGeneralLoadingPanel();}
if(closePopUp){PopupClose();}
return true;}
function RangeAddUpdateProdsInBasket(prodID,boxQty,oldQty,unitOfMeasure,inbasket,remarks,closePopUp){return GeneralProductAddUpdateToBasket(prodID,boxQty,oldQty,unitOfMeasure,remarks,inbasket,closePopUp);}
function GeneralProductAddUpdateToBasket(productIDs,Qtys,OldQtys,unitofmeasures,remarks,inbasket,closePopUp){ShowBasketLoadingPanel();if(IsExtended()){ShowBasketDetailsLoadingPanel();}
