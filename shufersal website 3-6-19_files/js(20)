function DoAddProductToBasket(prodID,qty,remarks,unitofmeasure,addProductToBasketCallback){if(isHD){if(addProductToBasketCallback.name!="AfterUpdate"){var x=document.getElementById("basketFrame");var ss=x.contentWindow.angular.element(x.contentWindow.document.querySelector('[ng-controller=TypeaheadCtrl]')).scope();ss.addProductById(prodID,qty,remarks)}
$('.miniBasketFooter').addClass('loadFooter');}
if(!isHD){ShowBasketLoadingPanel();}
HideAnonymouseUserPopup();if(!isNaN(qty)&&qty>0){$.post(GetCurrentPageUrlString(),{AjaxCallAction:"AddProductToBasket",paramProductID:prodID,paramQuantity:qty,paramRemarks:remarks,paramUM:unitofmeasure},function(data){if(data.indexOf("SessionExpired")==0){DoSessionExpired();return;}
else{if(data.indexOf('שגיאהכמות:')==0){ShowLimitedAmountsPopup();HideBasketLoadingPanel();HideBasketDetailsLoadingPanel();return;}
HideBasketLoadingPanel();if(data.indexOf('שגיאה: ')==0){ShowErrMsg('שגיאה',data.replace('שגיאה: ',''));return;}
if(typeof(addProductToBasketCallback)=='function'){addProductToBasketCallback(data,prodID,qty);}}});}
else{HideGeneralLoadingPanel();}}
function GetBasketProducts(){var sProducts=$.ajax({url:GetCurrentPageUrlString(),global:false,type:"POST",data:({AjaxCallAction:"GetBasketProducts"}),dataType:"text",async:false}).responseText;if(sProducts.indexOf("SessionExpired")==0){DoSessionExpired();return;}
return sProducts;}
function IsMaxNumberOfProductsReached(arrIDs,showError){if(showError){$('#listAddToBasketError').html('');$('#listAddToBasketError').parent().hide();$('#divPromoValidateWarning').hide();}
var ProductsInBasket=eval('('+ GetBasketProducts()+')');var maxNumberOfProducts=99;if(typeof(ProductsInBasket)!='undefined')
maxNumberOfProducts=ProductsInBasket.MaxItemsInBasket;if(maxNumberOfProducts<arrIDs.length){if(showError){$('#listAddToBasketError').html('כמות הפריטים בסל מוגבלת ל - '+ maxNumberOfProducts +' פריטים. לא ניתן להוסיף יותר מ '+(maxNumberOfProducts-ProductsInBasket.ItemIDsInBasket.length)+' פריטים');$('#listAddToBasketError').parent().show();if($('#divPromoValidateWarning').length>0){$('#divPromoValidateWarning').html('כמות הפריטים בסל מוגבלת ל - '+ maxNumberOfProducts +' פריטים. לא ניתן להוסיף יותר מ '+(maxNumberOfProducts-ProductsInBasket.ItemIDsInBasket.length)+' פריטים').show();}}
return true;}
var intersetArray=intersect(arrIDs,ProductsInBasket.ItemIDsInBasket);if(maxNumberOfProducts<ProductsInBasket.ItemIDsInBasket.length + arrIDs.length-intersetArray.length){if(showError){$('#listAddToBasketError').html('כמות הפריטים בסל מוגבלת ל - '+ maxNumberOfProducts +' פריטים. לא ניתן להוסיף יותר מ '+(maxNumberOfProducts-ProductsInBasket.ItemIDsInBasket.length)+' פריטים');$('#listAddToBasketError').parent().show();if($('#divPromoValidateWarning').length>0){$('#divPromoValidateWarning').html('כמות הפריטים בסל מוגבלת ל - '+ maxNumberOfProducts +' פריטים. לא ניתן להוסיף יותר מ '+(maxNumberOfProducts-ProductsInBasket.ItemIDsInBasket.length)+' פריטים').show();}}
return true;}
return false;}
function intersect(array1,array2){var intersection=[];for(i in array1){for(j in array2){if(array1[i]==array2[j])intersection.push(array1[i]);}}
return intersection;}
function DoAddUpdateProdArrayToBasket(prodIDs,qtys,oldqtys,remarks,unitofmeasure,isinbasket,addUpdateProdArrayToBasketCallback,closePopUp){if(IsMaxNumberOfProductsReached(prodIDs,true)){return false;}
ShowBasketLoadingPanel();HideAnonymouseUserPopup();var sendClearBasket='false';if(document.getElementById('ctl00_PlaceHolderMain_ucPopupMasterLoader_ucListAddToBasketPopup_hdClearBasket')&&document.getElementById('ctl00_PlaceHolderMain_ucPopupMasterLoader_ucListAddToBasketPopup_hdClearBasket')!='undefined'){var clearBasket=document.getElementById('ctl00_PlaceHolderMain_ucPopupMasterLoader_ucListAddToBasketPopup_hdClearBasket').value;if(clearBasket=='true'){sendClearBasket='true';}}
if(prodIDs.length>0){$.post(GetCurrentPageUrlString(),{AjaxCallAction:"AddUpdateProdArrayToBasket",paramProductID:prodIDs.join(SEPARATOR),paramQuantity:qtys.join(SEPARATOR),paramOldQuantity:oldqtys.join(SEPARATOR),paramRemarks:remarks.join(SEPARATOR),paramUM:unitofmeasure.join(SEPARATOR),paramIsInBasket:isinbasket.join(SEPARATOR),paramClearBasket:sendClearBasket},function(data){if(data.indexOf("SessionExpired")==0){DoSessionExpired();return;}
else{if(data.indexOf('שגיאהכמות:')==0){ShowLimitedAmountsPopup('true');HideBasketLoadingPanel();HideGeneralLoadingPanel();HideBasketDetailsLoadingPanel();WT_list_Mng_AddFromListToBasket('failed',data);return;}
HideBasketLoadingPanel();if(data.indexOf('שגיאה: ')==0){ShowErrMsg('שגיאה',data.replace('שגיאה: ',''));WT_list_Mng_AddFromListToBasket('failed',data);return;}
if(typeof(addUpdateProdArrayToBasketCallback)=='function'){addUpdateProdArrayToBasketCallback(data);if(isHD){document.getElementById("basketFrame").contentDocument.location.reload(true);}
PopupClose();WT_list_Mng_AddFromListToBasket('success');}}});}
else{HideGeneralLoadingPanel();}
if(closePopUp){PopupClose();}
return true;}
function ProdPage_AddProductToBasket(prodID){var Qty=$("input[id$='prodpageqtybox']");if(!ValidateMaxQty(Qty.val(),ITEM_MAX_QTY)){return false;}
var unitofmeasure=GetUnitOfMeasureById('#ddlUMProdPage','#divUMProdPage');var remarks=$('input[id$=txtRemarks]').val();remarks=remarks=='הכנס הערה'?'':remarks;if(Qty.val()&&!isNaN(Qty.val())){DoAddProductToBasket(prodID,Qty.val(),remarks,unitofmeasure,ProdPage_AddProductToBasketCallBack);if(IsExtended())
RefreshhOpenListAfterAllProductsAreUpdated();}
try{GTMAddProductToCart('ProdPage',prodID,$('input[id$=hdnProductName]').val(),$('input[id$=hdnProductPrice]').val(),$("span.[id$='lblSupplier']").text(),m_sSelectedCategory,unitofmeasure,Qty.val())}
catch(e){}}
function ProdPage_AddProductToBasketCallBack(data,prodID,currentQty){GeneralAddToBasketCallBack(data,prodID,currentQty);$('div[id$=btnprodpageadd]').hide();$('div[id$=btnprodpageupdate]').show();$('.prodmsgaddedsh').show().delay(1000);if(!$('.prodmiddleline').hasClass('prodmiddlelineNF'))$('.prodmiddleline').css("background-color","#DFEDED");$("input[id$='prodpageqtybox']").attr('oldqty',$("input[id$='prodpageqtybox']").val());if(document.location.toString().toLowerCase().indexOf("order.aspx")>-1){window.location.href='catalog.aspx';}}
function ProductActionRemoveHighlight(prodID){var prodRemovedText="פריט הוסר";if($("#divProduct_"+ prodID).length>0){oProductQuantity=$("#divProductQuantity_"+ prodID);if(oProductQuantity!=null&&oProductQuantity.length>0){oProductQuantity.hide();$("#divProduct_"+ prodID +" div[id='divProductDetailsTexts']").hide();$("#divProduct_"+ prodID +" div[id='divProductDetails']").removeClass().addClass('Float PWdivProductDetailsYellow');ShowProdActionInfoMsg(prodRemovedText,prodID);setTimeout("RestoreOriginalProductArea('"+ prodID +"');",2000);oProductQuantity.show();}
ResetProdQty(prodID)}}
function ResetProdQty(prodID){var QtyAdd=$("input[id='ProdAddQuantityTextBox_"+ prodID +"']");var QtyUpdate=$("input[id='ProdUpdateQuantityTextBox_"+ prodID +"']");QtyAdd.val("1");QtyUpdate.val("1");if($('#ddlUMChartViewAdd_'+ prodID).length>0){var increment=parseFloat($('#ddlUMChartViewAdd_'+ prodID +' option:selected').attr('increment'));if(isNaN(increment))
increment=1;QtyAdd.val(increment);QtyUpdate.val(increment);QtyAdd.attr('iq',increment);QtyUpdate.attr('iq',increment);}}
function ProductActionHighlight(prodID,actionText){oProductQuantity=$("#divProductQuantity_"+ prodID);oProductAddToBasket=$("#divProductAddToBasket_"+ prodID);if(oProductQuantity!=null&&oProductAddToBasket!=null&&oProductQuantity.length>0&&oProductAddToBasket.length>0){oProductAddToBasket.hide();oProductQuantity.hide();$("#divProduct_"+ prodID +" div[id='divProductDetailsTexts']").hide();$("#divProduct_"+ prodID +" div[id='divProductDetails']").removeClass().addClass('Float PWdivProductDetailsYellow');ShowProdActionInfoMsg(actionText,prodID);setTimeout("RestoreProductArea('"+ prodID +"');",2000);SetIncreaseQty("ddlUMChartViewAdd_"+ prodID,"ProdUpdateQuantityTextBox_"+ prodID);oProductQuantity.show();}
else{if($("div[class^='productRow'][pid='"+ prodID +"']").length>0){setTimeout("RestoreBasketItemLine('"+ prodID +"');",2000);}}}
function SetIncreaseQty(ddl,qtyboxid){if($('#'+ ddl).length>0){var increment=parseFloat($('#'+ ddl +' option:selected').attr('increment'));if(isNaN(increment))
increment=1;$('#'+ qtyboxid).attr('iq',increment);}}
function RestoreBasketItemLine(prodID){$('#dvMinibasketContent').find("div[class^='productRow'][pid='"+ prodID +"']").attr("class","productRow");}
function RestoreOriginalProductArea(prodID){Prod_Show_UM_DDL("ddlUMChartViewUpdate_","spnUmChartUpdate_",prodID,false);HideProdActionInfoMsg(prodID);$("#divProductQuantity_"+ prodID).hide();$("#divProduct_"+ prodID +" div[id='divProductDetailsTexts']").show();$("#divProductPricePerUnit_"+ prodID).show();$("#divProduct_"+ prodID +" div[id='divProductDetails']").removeClass().addClass('Float PWdivProductDetails');}
function RestoreProductArea(prodID){$("#divProduct_"+ prodID +" div[id='divProductDetailsTexts']").show();HideProdActionInfoMsg(prodID);$("#divProduct_"+ prodID +" div[id='divProductDetails']").removeClass().addClass('Float PWdivProductDetailsAdd');$('#dvMinibasketContent').find("div[class^='productRow'][pid='"+ prodID +"']").attr("class","productRow");}
function Prod_Hide_UM_DDL(ddlID,spanID,prodId){if($("#"+ ddlID + prodId).length>0){if($("#ddlUMminibasket_"+ prodId).length>0){var selectedVal=$("#ddlUMminibasket_"+ prodId).val();if(selectedVal!=''){$("#"+ ddlID + prodId).attr('skip','1').val(selectedVal).change();}}
$("#"+ ddlID + prodId).hide();if($("#"+ spanID + prodId).length>0){$("#"+ spanID + prodId).text($("#"+ ddlID + prodId +" option:selected").text());}
else{$("#"+ ddlID + prodId).after("<span id='"+ spanID + prodId +"'>"+ $("#"+ ddlID + prodId +" option:selected").text()+"</span>");}}}
function Prod_Show_UM_DDL(ddlID,spanID,prodId,showUM){if($("#"+ ddlID + prodId).length>0){if(showUM){$("#"+ ddlID + prodId).show();}
else{$("#"+ ddlID + prodId).hide();}
if($("#"+ spanID + prodId).length>0){$("#"+ spanID + prodId).remove();}}}
function pcupd(prodID){if($("input[id=ProdUpdateQuantityTextBox_"+ prodID +"].BasketQtyInput").length>0){var Qty=$("input[id=ProdUpdateQuantityTextBox_"+ prodID +"].BasketQtyInput");var unitofmeasure=GetUnitOfMeasureById('#ddlUMChartViewUpdate_'+ prodID,'#divUMChartView_'+ prodID);if(Qty.val()&&!isNaN(Qty.val())){var myval=Qty.val();if(!ValidateMaxQty(myval,GetMaxQtyByUM(GetUnitOfMeasureById('#ddlUMChartViewAdd_'+ prodID,'#divUMChartView_'+ prodID))))return;$("div[class^='productRow'][pid='"+ prodID +"']").find("input.number").val(myval);DoUpdateProductToBasket(prodID,Qty.attr('oldqty'),Qty.val(),'',unitofmeasure,ProductChartUpdateToBasketCallback);RefreshMiniBasketFooter();}}}
