if(step==6&&jQuery.browser.msie!=undefined)
oPopup.css({"top":42,"left":(oElement.offset().left==0?oElement.width()-150 + iAddedLeft:oElement.offset().left)-350 + iAddedLeft});}
else{oPopup.center({minX:-350});}}
oPopup.show(1000);if(step==3){OnKeyUpFunction(null,"frombuble");}});}});}
