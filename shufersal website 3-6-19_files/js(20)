else{$(".miniBasketQClicked span[class^='QAprove']").css("display","none");$(".miniBasketQClicked").attr("class","miniBasketQ");e.stopPropagation();}});}}
function UrlExists(url){var http=new XMLHttpRequest();http.open('HEAD',url,false);http.send();return http.status!=404;}
