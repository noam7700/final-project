function CheckURLQueryParams(){var pid,cid,promoid,action,keyWord,banner;var url=$(location).attr('href');if(url!=undefined&&url.toLowerCase().indexOf('/catalog.aspx')>0){if(pid=$.getQueryString('productpage')){if(cid=$.getQueryString('cid')){var lvlid=$.getQueryString('lvlid');var parentCatId=$.getQueryString('prid');var mainCatId=$.getQueryString('mid');var manufactureCode=$.getQueryString('manuf');var oLink2=$("div.scc a[lv2cat='"+ parentCatId +"']");$("div.ci[itemid='"+ mainCatId +"'] div.cii").attr("class","cis")
SCS(oLink2,mainCatId,parentCatId,2,mainCatId,parentCatId,true);var oLink3=$("div.scc a[lv3cat='"+ cid +"']");SCS(oLink3,mainCatId,cid,3,mainCatId,cid,true);}
else{var menuWrapper=$('#BackgroundMenu');menuWrapper.addClass("HideSubmenuRow");menuWrapper.css({"height":"0px","min-height":"0px"});}
$("#prodWrapperScroll").height($(window).height()-$(".rgDataDiv").offset().top-140);$("#ctl00_PlaceHolderMain_ucMain_ctlProductsView_ProductPanel").css('display','block');return true;}
else if(pid=$.getQueryString('pid')){if(cid=$.getQueryString('cid')){MarkCategoryAsSelected(cid,null,true);ShowProductPagePopup(pid);return true;}
CatalogGoOnDefaultCategory();ShowProductPagePopup(pid);}
else if(promoid=$.getQueryString('promoid')){var productID=GetProductIDByPromoID(promoid);if(productID!="-1"){if(productID=="0"){DisplayRangePopup(promoid);}
else
ShowProductPagePopup(productID);if(cid=$.getQueryString('cid')){MarkCategoryAsSelected(cid,null,true);ShowProductPagePopup(productID);return true;}
CatalogGoOnDefaultCategory();return true;}}
else if(action=$.getQueryString('action')){if(action.toLowerCase()=='registercc'){OpenIframeCC();return true;}
if(action.toLowerCase()=='showexclusivepromos'){MarkCategoryAsSelected('pr0',lvlid,false);$('.promoFilterWrapper :radio[value="3"]').attr('checked','checked');OnSelectedFilterClick()
return true;}
if(action.toLowerCase()=='showspecpromos'){MarkCategoryAsSelected('pr0',lvlid,false);$('.promoFilterWrapper :radio[value="1"]').attr('checked','checked');OnSelectedFilterClick()
return true;}
if(action.toLowerCase()=='showweeklypromos'){MarkCategoryAsSelected('pr0',lvlid,false);$('.promoFilterWrapper :radio[value="2"]').attr('checked','checked');OnSelectedFilterClick()
return true;}
else if(action.toLowerCase()=='recipe'){var products=$.getQueryString('products');ShowListAddToBasketFromProductsPopup(products,"1");$("div[id$=radGeneralLoad]").hide();return false;}
else if(action.toLowerCase()=='addtobasket'){var products=$.getQueryString('basket');ShowListAddToBasketFromProductsPopup(products,"2");return false;}
else if(action.toLowerCase()=='zapaddtobasket'){var products=$.getQueryString('basket');ShowListAddToBasketFromProductsPopup(products,"4");return false;}
else if(action.toLowerCase()=="ashlyaddtobasket"){var products=$.getQueryString('basket');ShowListAddToBasketFromProductsPopup(products,"5");return false;}
else if(action.toLowerCase()=="newpharmaddtobasket"){var products=$.getQueryString('basket');ShowListAddToBasketFromProductsPopup(products,"6");$("div[id$=radGeneralLoad]").hide();return false;}
else if(action.toLowerCase()=="pricesaddtobasket"){var products=$.getQueryString('basket');ShowListAddToBasketFromProductsPopup(products,"7");$("div[id$=radGeneralLoad]").hide();return false;}
else if(action.toLowerCase()=='addlist'){var products=$.getQueryString('products');ShowListAddToBasketFromProductsPopup(products,"3");$("div[id$=radGeneralLoad]").hide();return false;}
else if(action.toLowerCase()=='coupons'){if(isAnonymous())
OpenCoupons(true);else
OpenCoupons(false);return false;}
else if(action.toLowerCase()=='RaiseCouponsPopup'){ShowDirbunForCCPopup(8,8)}
else if(action.toLowerCase()=='feedcoupon'||action.toLowerCase()=='searchcoupon'){if(isAnonymous())
OpenCoupons(true);else{couponUrlSourceFlag=true;GetCurrentBasket();ShowCouponsPopup();}
return false;}
else if(action.toLowerCase()=='smartlist'){MinibasketAnonymusFillBasket('');return false;}}
else if(keyWord=$.getQueryString('KeyWord')){$('input[id$=hidHidePnlFilterSearch]').val('true');FillSearchResultProducts(keyWord,true);return true;}
else if(banner=$.getQueryString('banner')){if(banner==='3'){ShowPromoPopup(banner,true);}else{ShowPromoPopup(banner);}}
else{var lvlid=$.getQueryString('lvlid');var parentCatId=$.getQueryString('prid');var mainCatId=$.getQueryString('mid');var manufactureCode=$.getQueryString('manuf');if(cid=$.getQueryString('cid')){if(lvlid!=undefined&&parentCatId!=undefined&&mainCatId!=undefined){if(manufactureCode!=undefined){SetFilteredManufacturer('B_'+ manufactureCode,cid,lvlid,false);}
else{OnSubcategClick(cid,mainCatId,parentCatId,lvlid,mainCatId);}}
else{MarkCategoryAsSelected(cid,lvlid,true);}
return true;}}}
return false;}
