if(m_sCurrentDisplayMode=='promo')
aditionHeight=161;var bannerHeight=GetCatalogBannersHeight();aditionHeight +=bannerHeight;iHeight-=aditionHeight;if(m_sCurrentDisplayLevel=='1'&&(jQuery(window).width()+ 25<jQuery(document).width())){iHeight +=16;}
else if(IsIe()&&m_sCurrentDisplayMode!='promo'){iHeight +=12;}
return iHeight;}
function CalculateWidth(){if(Ie7())return CalculateWidthIE7();var rightWidth=jQuery(".CategoryBgV2Dark").width();if(jQuery(".PWdivProduct").length==0&&m_sCurrentDisplayMode!='promo'){return parseInt((parseInt(jQuery(document).width())-rightWidth)/(134 + 8));}
var itemWidth=0;if(jQuery(".PWdivProduct").length>0){itemWidth=jQuery(".PWdivProduct").css("width").replace("px","");}
if(m_sCurrentDisplayMode=='promo'||m_ShowOnlyDeals=='1')
itemWidth=300;var itemMarginLeft=0;var itemMarginRight=0;if(jQuery(".PWdivProduct").length>0){itemMarginLeft=jQuery(".PWdivProduct").css("margin-left").replace("px","");itemMarginRight=jQuery(".PWdivProduct").css("margin-right").replace("px","");}
if(itemWidth.length>0){itemWidth=parseInt(itemWidth);}
if(itemMarginLeft.length>0){itemMarginLeft=parseInt(itemMarginLeft);}
if(itemMarginRight.length>0){itemMarginRight=parseInt(itemMarginRight);}
var iRowItems=(parseInt(jQuery(document).width())-rightWidth)/(itemWidth + itemMarginRight + itemMarginLeft);return parseInt(iRowItems);}
function CalculateWidthIE7(){var rightWidth=jQuery("#tdMiniBasket").width();var hiddenProdDiv=jQuery("#hereOnlyForID");var prodDiv=hiddenProdDiv.parent();if(hiddenProdDiv.length==0&&m_sCurrentDisplayMode!='promo'){return parseInt((parseInt(jQuery(document).width())-rightWidth)/(134 + 8));}
var itemWidth=0;if(prodDiv.length>0){itemWidth=prodDiv.css("width").replace("px","");}
if(m_sCurrentDisplayMode=='promo'||m_ShowOnlyDeals=='1')
itemWidth=300;var itemMarginLeft=0;var itemMarginRight=0;if(prodDiv.length>0){itemMarginLeft=prodDiv.css("margin-left").replace("px","");itemMarginRight=prodDiv.css("margin-right").replace("px","");}
if(itemWidth.length>0){itemWidth=parseInt(itemWidth);}
if(itemMarginLeft.length>0){itemMarginLeft=parseInt(itemMarginLeft);}
if(itemMarginRight.length>0){itemMarginRight=parseInt(itemMarginRight);}
var iRowItems=(parseInt(jQuery(document).width())-rightWidth)/(itemWidth + itemMarginRight + itemMarginLeft);return parseInt(iRowItems);}
