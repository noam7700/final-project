oCtrl.css({"top":iHeight +"px","left":iWidth +"px"});}
bCloseAlertMask=false;function ShowGenericAlert(message,okText){var oMask=$("#popupBoxMask");if(oMask.length>0){if(oMask.css("display").toLowerCase()=='inline'||oMask.css("display").toLowerCase()=='block')
bCloseAlertMask=false;else
bCloseAlertMask=true;oMask.css({"width":jQuery(window).width()+"px","height":jQuery(window).height()+"px","z-index":"10002","top":"0px","left":"0px"}).show();}
