else{PopupClose();}}
function GeneralProductAddUpdateToBasketCallBack(data){var tokens=data.split('|#|',3);if(tokens.length>2){var added=tokens[0];var updated=tokens[1];var html=tokens[2];var addedTokens=added.split('|@|');for(var index=0;index<addedTokens.length;index +=2){if(addedTokens.length>index + 1){if(index>=addedTokens.length-3)
GeneralAddToBasketCallBack(html,addedTokens[index],addedTokens[index + 1]);else
GeneralAddToBasketCallBack('',addedTokens[index],addedTokens[index + 1]);}}
var updatedTokens=updated.split('|@|');for(var index=0;index<updatedTokens.length;index +=2){if(updatedTokens.length>index + 1){if(index>=updatedTokens.length-3)
GeneralUpdateToBasketCallback(html,updatedTokens[index],updatedTokens[index + 1]);else
GeneralUpdateToBasketCallback('',updatedTokens[index],updatedTokens[index + 1]);}}
if(IsExtended()){ShowBasketDetailsLoadingPanel();setTimeout("RefreshhOpenListAfterAllProductsAreUpdated()",200);}}}
function GeneralProductAddToBasket(productID,Qty,unitofmeasure,remarks,callBackMethod){if(typeof(callBackMethod)==typeof(undefined)||callBackMethod==""){callBackMethod=GeneralAddToBasketCallBack;}
if(document.location.toString().toLowerCase().indexOf("catalog.aspx")==-1){ShowGeneralLoadingPanel();}
if(!isHD){ShowBasketLoadingPanel();}
HideAnonymouseUserPopup();if(!isNaN(Qty)&&productID.length>0){DoAddProductToBasket(productID,Qty,remarks,unitofmeasure,callBackMethod);}}
function GeneralAddToBasketCallBack(data,prodID,currentQty){if(document.location.toString().toLowerCase().indexOf("order.aspx")>-1||document.location.toString().toLowerCase().indexOf("myaccount.aspx")>-1){window.location.href='catalog.aspx';}
if(document.location.toString().toLowerCase().indexOf("catalog.aspx")==-1){HideGeneralLoadingPanel();return;}
ClientAddToMiniBasket_v2(data,'','',prodID);$('#dvMinibasketContent').find("div[class='productRow'][pid='"+ prodID +"']").attr("class","productRow productRowHighlight");HighlightProductsGrid(prodID,currentQty,true);ChangePromoDisplayMode(true,prodID,currentQty);}
