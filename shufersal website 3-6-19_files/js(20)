if(!ValidateMaxQty(oQty,GetMaxQtyByUM(GetUnitOfMeasureById('#ddlUMminibasket_'+ oID,'#divUMminibasket_'+ oID))))return;oProdQtyCtrl.val(oQty.toFixed(2));}}
