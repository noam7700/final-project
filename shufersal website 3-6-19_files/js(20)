function validateIsracard(creditCardNumber){var length=creditCardNumber.length;if(length==8||length==9){creditCardNumber=(length==8?"0":"")+ creditCardNumber;length=creditCardNumber.length;var weightNumber=1;var currIndex=length-1;var sumNum=0;for(var i=currIndex;i>=0;i--){sumNum +=weightNumber*parseFloat(creditCardNumber.charAt(i));weightNumber++;}
return sumNum%11==0;}
else
return false;}
function addCommas(nStr){nStr +='';x=nStr.split('.');x1=x[0];x2=x.length>1?'.'+ x[1]:'';var rgx=/(\d+)(\d{3})/;while(rgx.test(x1)){x1=x1.replace(rgx,'$1'+','+'$2');}
return x1 + x2;}
