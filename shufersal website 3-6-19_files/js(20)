function DelProductFromList(){var pid;pid=$(globalClientListID).closest('.ProducRow').attr('pid');if($(".OrderBasketWrapper").is(":visible")){var listName=$(".OrderBasketInnerWrapper .MainTitle").text().replace('הוספת פריטים - ','');if($('input[id$=hdnIsMyDWHList]').val()=='true')
listName='IsMyDWHList'}
$.post(GetCurrentPageUrlString(),{AjaxCallAction:"DeleteItemsFromShopperList",ListName:listName,ItemID:pid},function(data){if(data.indexOf("SessionExpired")==0){DoSessionExpired();return;}
else{if(data.indexOf('שגיאה: ')==0){ShowErrMsg('שגיאה',data.replace('שגיאה: ',''));return;}
else{var line=$(globalClientListID).closest('.ProducRow');line.remove();}}});}
