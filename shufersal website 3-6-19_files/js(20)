function searchCoupon(couponid){var couponId=couponid||$('input[id$=txtSearch]').val();var msg=jQuery('div[id=errorRemark]');msg[0].innerHTML="";if(!RegExpMatch(couponId,"^[0-9]{3,20}$")){msg[0].innerHTML="נא להזין מספר קופון תקין";msg.css("display","block");}
else{msg.css("display","none");ShowBasketLoadingPanel();$.post(GetCurrentPageUrlString(),{AjaxCallAction:"searchCoupon",paramCouponId:couponId},function(data){HideBasketLoadingPanel()
