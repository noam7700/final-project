SendWT('WT.yashiraction,WT.yashirsection,WT.fixed_list_type','select all products chbx,fixed lists,'+ type);else if(mode=='unselect')
SendWT('WT.yashiraction,WT.yashirsection,WT.fixed_list_type','uncheck all products chbx,fixed lists,'+ type);}
