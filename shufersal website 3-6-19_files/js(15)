WebTrends.prototype.customCollection=function(keysList,valuesList){var keysArray=keysList.split(',');var valuesArray=valuesList.split(',');var counter=keysArray.length;var str="this.dcsMultiTrack('DCS.dcsuri','/Catalog/PopupBox'";if(keysArray.length==valuesArray.length&&counter>0){for(var i=0;i<counter;i++){str +=", '"+ keysArray[i]+"', '"+ valuesArray[i]+"'";}
str +=");";eval(str);}}
function SendWT(keyList,valuesList){$.post(GetCurrentPageUrlString(),{AjaxCallAction:"CheckSession"},function(data){if(data.indexOf("SessionExpired")==0){DoSessionExpired();return;}
else{var _wt=new WebTrends();_wt.dcsGetId();_wt.customCollection(keyList,valuesList);}});}
